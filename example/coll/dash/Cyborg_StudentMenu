    <script type="text/javascript">


        $(document).keydown(function (event) {
            if (event.keyCode == 123) {
                return false;
            }
            else if (event.ctrlKey && event.shiftKey && event.keyCode == 73) {
                return false;
            }
            else if ((event.ctrlKey && event.keyCode == 117) || (event.ctrlKey && event.keyCode == 85)) {
                return false;
            }
        });

        $(document).on("contextmenu", function (e) {
            e.preventDefault();
        });

        $(document).ready(function () {

            var config = "UU";
            var url = window.location.href;
            if (config == "QGC") {
                $("#srcicon").attr("href", "../icon/QGCico.ico");

            } else if (config == "GEU") {
                if (url.search("geu") != -1) {
                    $("#srcicon").attr("href", "../icon/GEUico.ico");
                }
                else if (url.search("gehu") != -1) {
                    $("#srcicon").attr("href", "../icon/GEHUico.ico")
                } else {
                    $("#srcicon").attr("href", "../icon/GEUico.ico");
                }
            } else if (config == "UU") {
                $("#srcicon").attr("href", "../icon/UUico.ico")
            } else if (config == "RIT") {
                $("#srcicon").attr("href", "../icon/RITico.ico")
            } else if (config == "SCE") {
                $("#srcicon").attr("href", "../icon/SCEico.ico")
            } else if (config == "GRD") {
                $("#srcicon").attr("href", "../icon/GRD.ico")
            } else if (config == "COER") {
                $("#srcicon").attr("href", "../icon/COERico.ico")
            } else if (config == "BFIT") {
                $("#srcicon").attr("href", "../icon/BFIT.ico")
            } else if (config == "CGC") {
                $("#srcicon").attr("href", "../icon/CGC.ico")
            } else {
                $("#srcicon").attr("href", "../icon/Cyborgico.ico");
            }


            var Msg = "UU";

            if (Msg == 'UU') {
                $("#lnkDashboard").hide();
                $("#dvstuattendance").show();
                $("#dvdashboard").hide();
            } else {
                $("#lnkDashboard").show();
                $("#dvstuattendance").hide();
                $("#dvdashboard").show();
            }

            if (Msg == 'UU') {
                $("#lnkDashboard1").hide();
            } else {
                $("#lnkDashboard1").show();
            }

            if (Msg == 'UU') {
                $("#lnkDashboardMobile").hide();
            } else {
                $("#lnkDashboardMobile").show();
            }



            

            var appurl = $(location).attr("href");
            var n = String(appurl).includes("app.");
            if (n == true) {
                $("#dvlistbar").hide();
            }

            var appurl = $(location).attr("href");
            var n = String(appurl).includes("app.");
            if (n == true) {
                $("#dvlistbar1").hide();
            }
            var appurl = $(location).attr("href");
            var n = String(appurl).includes("app.");
            if (n == true) {
                $("#dvlistbarMobile").hide();
            }



            $("#lnkReturn").click(function () {
                //window.location.href = 'Account/Cyborg_MenuLogin';
                var appurl = $(location).attr("href");
                var n = String(appurl).includes("app.");

                if (n == true) {
                    var myUrl = '/Account/APP_StudentHome';
                    window.location.href = myUrl;
                }
                else {
                    var clg = "UU";
                    if (clg == "UU") {
                        var CollegeID = "30";
                        if (CollegeID == "32") {
                            var myUrl = '/Account/Cyborg_OnlineStudentMenu';
                            window.location.href = myUrl;
                        }
                        else {
                            var myUrl = '/Account/Cyborg_StudentMenu';
                            window.location.href = myUrl;
                        }
                    }
                    else {
                        var myUrl = '/Account/Cyborg_StudentMenu';
                        window.location.href = myUrl;
                    }

                }
            });

            $("#lnkReturn1").click(function () {
                //window.location.href = 'Account/Cyborg_MenuLogin';
                var appurl = $(location).attr("href");
                var n = String(appurl).includes("app.");
                if (n == true) {
                    var myUrl = '/Account/APP_StudentHome';
                    window.location.href = myUrl;
                } else {
                    var clg = "UU";
                    if (clg == "UU") {
                        var CollegeID = "30";
                        if (CollegeID == "32") {
                            var myUrl = '/Account/Cyborg_OnlineStudentMenu';
                            window.location.href = myUrl;
                        }
                        else {
                            var myUrl = '/Account/Cyborg_StudentMenu';
                            window.location.href = myUrl;
                        }
                    }
                    else {
                        var myUrl = '/Account/Cyborg_StudentMenu';
                        window.location.href = myUrl;
                    }
                }
            });

            $("#lnkDashboard").click(function () {
                //window.location.href = 'Account/Cyborg_MenuLogin';
                var myUrl = '/Web_StudentAcademic/Cyborg_S_Dashboard';
                window.location.href = myUrl;
            });

            $("#lnkDashboardMobile").click(function () {
                //window.location.href = 'Account/Cyborg_MenuLogin';
                var myUrl = '/Web_StudentAcademic/Cyborg_S_Dashboard';
                window.location.href = myUrl;
            });


            $("#lnkDashboard1").click(function () {
                //window.location.href = 'Account/Cyborg_MenuLogin';
                var myUrl = '/Web_StudentAcademic/Cyborg_S_Dashboard';
                window.location.href = myUrl;
            });

        });
    </script>

    <script type="text/javascript">
        $(document).ready(function () {
            var url = $(location).attr('pathname');
            if (url == '/Account/Cyborg_MenuLogin' || url == '/Account/Cyborg_StudentMenu') {

                var logo = $(".navbar > .container .navbar-brand");
                logo.addClass('active');;
            }

            $("#lnkapple").hide();
            $("#lnkapple1").hide();

            var clg = "UU";
            var clientimg = document.getElementById('imgclient');
            var imgclientmobile = document.getElementById('imgclientmobile');

            if (clg == "GEU") {
                clientimg.style.height = '55px';
                clientimg.style.width = '175px';
                clientimg.style.marginTop = '-6px';
                imgclientmobile.style.height = '55px';
                imgclientmobile.style.width = '175px';
                imgclientmobile.style.marginTop = '-6px';
            }

            if (clg == "COER") {
                clientimg.style.height = '51px';
                clientimg.style.width = '194px';
                clientimg.style.marginTop = '-6px';
                imgclientmobile.style.height = '56px';
                imgclientmobile.style.width = '181px';
                imgclientmobile.style.marginTop = '-14px';
            }

            if ((clg == "RIT" || clg == "QGC" || clg == "SCE")) {
                $("#app-dwnld").show();
                $("#app-dwnld1").show();
            } else {
                $("#app-dwnld").hide();
                $("#app-dwnld1").hide();
            }

            $("#lnkandroid").click(function () {

                var config = "UU";
                var url = '';
                var filename = '';
                if (config == "UU") {
                    var url = 'https://play.google.com/store/apps/details?id=appinventor.ai_campsyshelpdesk.UU';
                    window.location.href = url;
                }
                if (config == "RIT") {
                    var url = 'https://drive.google.com/file/d/1W6Lh2LnXKad1g_zY9BRgBIkwkRnRFxy7/view?usp=sharing';
                    window.location.href = url;
                }
                if (config == "QGC") {
                    var url = 'https://play.google.com/store/apps/details?id=appinventor.ai_campsyshelpdesk.QU';
                    window.location.href = url;
                }
                if (config == "SCE") {
                    var url = 'https://drive.google.com/file/d/1QR3k7_FiqgSafybHcgvw-7uteB6BU_NS/view?usp=sharing';
                    window.location.href = url;
                }


                //fetch(url)
                //   .then(resp => resp.blob())
                //   .then(blob => {
                //       const url = window.URL.createObjectURL(blob);
                //       const a = document.createElement('a');
                //       a.style.display = 'none';
                //       a.href = url;
                //       // the filename you want
                //       a.download = filename;
                //       document.body.appendChild(a);
                //       a.click();
                //       window.URL.revokeObjectURL(url);
                //       // alert('your file has downloaded!'); // or you know, something with better UX...
                //   })
                //   .catch(() => alert('Some error occured in download.'));
            });


            $("#lnkandroid1").click(function () {

                var config = "UU";
                var url = '';
                var filename = '';
                if (config == "UU") {
                    var url = 'https://play.google.com/store/apps/details?id=appinventor.ai_campsyshelpdesk.UU';
                    window.location.href = url;
                }
                if (config == "RIT") {
                    var url = 'https://drive.google.com/file/d/1W6Lh2LnXKad1g_zY9BRgBIkwkRnRFxy7/view?usp=sharing';
                    window.location.href = url;
                }
                if (config == "QGC") {
                    var url = 'https://play.google.com/store/apps/details?id=appinventor.ai_campsyshelpdesk.QU';
                    window.location.href = url;
                    $("#dvstuattendance").show();
                    $("#dvdashboard").show();

                }
                if (config == "SCE") {
                    var url = 'https://drive.google.com/file/d/1QR3k7_FiqgSafybHcgvw-7uteB6BU_NS/view?usp=sharing';
                    window.location.href = url;
                }


                //fetch(url)
                //   .then(resp => resp.blob())
                //   .then(blob => {
