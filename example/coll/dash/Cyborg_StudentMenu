<script>
    var coll = document.getElementsByClassName("collapsible");
    var i;

    for (i = 0; i < coll.length; i++) {
        coll[i].addEventListener("click", function () {
            this.classList.toggle("active");
            var content = document.querySelector(".content");
            if (content.style.maxHeight) {
                content.style.maxHeight = null;
            }
            else {
                content.style.maxHeight = 0 + "px";
            }
        });
    }



    //$("ol").on("click", ".init", function () {
    //    $(this).closest("ol").children('li:not(.init)').toggle();
    //});

    //var allOptions = $("ol").children('li:not(.init)');
    //$("ol").on("click", "li:not(.init)", function () {
    //    allOptions.removeClass('selected');
    //    $(this).addClass('selected');
    //    $("ol").children('.init').html($(this).html());
    //    allOptions.toggle();
    //});

</script>

<script>

    function isNumber(evt) {
        evt = (evt) ? evt : window.event;
        var charCode = (evt.which) ? evt.which : evt.keyCode;
        if (charCode == 8 || charCode == 46) {
            return true;
        }
        if (charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }
        if (charCode < 48 || charCode > 57) {
            return false;
        }
        return true;
    }

    // Function to filter and display div elements based on input text
    function filterHeadlines(inputText) {
        const divs = document.querySelectorAll(".content > .col-sm-12 >.row > .rightborder");

        divs.forEach(function (div) {
            const headlineText = div.querySelector(".headline").textContent.toLowerCase();
            if (headlineText.includes(inputText)) {
                div.style.display = "block"; // Show the div if it contains the input text
            } else {
                div.style.display = "none"; // Hide the div if it doesn't contain the input text
            }
        });
    }

    // Event listener for keyup event to filter on each keystroke
    var text = document.getElementById("searchInput");
    text.addEventListener("keyup", function (event) {
        const inputText = event.target.value.toLowerCase();
        filterHeadlines(inputText);
    });




    function StuCollaps(){
        $(".Stucontent").toggleClass('activeStu');;
    }

    
</script>




        

        
