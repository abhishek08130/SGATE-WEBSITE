<script>
    // When the user scrolls down 20px from the top of the document, slide down the navbar
    window.onscroll = function() {scrollFunction()};

    function scrollFunction() {
        if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
            document.getElementById("dvQUCLAlink").style.top = "-1%";
            document.getElementById("dvQUCLAlink").style.width = "80px";
            document.getElementById("dvQUCLAlink").style.height = "80px";

            document.getElementById("dvQUCLAlinkmobile").style.top = "-1%";
            document.getElementById("dvQUCLAlinkmobile").style.width = "80px";
            document.getElementById("dvQUCLAlinkmobile").style.height = "80px";

        } else {
            document.getElementById("dvQUCLAlinkmobile").style.top = "0px";
            document.getElementById("dvQUCLAlinkmobile").style.width = "80px";
            document.getElementById("dvQUCLAlinkmobile").style.height = "80px";

            document.getElementById("dvQUCLAlink").style.top = "0px";
            document.getElementById("dvQUCLAlink").style.width = "80px";
            document.getElementById("dvQUCLAlink").style.height = "80px";
        }
    }

    function blinktext() {
        $("#blink_mefirst").fadeOut(500);
        $("#blink_mefirst").fadeIn(500);
    }
    setInterval(blinktext, 1000);
</script>





