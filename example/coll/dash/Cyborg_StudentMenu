            $("#FileCertificate8").val(null);

        });

        $("#btnSub8").click(function()
        {
            $("#Row8").hide();
            $("#btnAdd7").show();
            $("#btnSub7").show();
            $("#txtCourseTitle8").val("");
            $("#txtProgramName8").val(0);
            $("#FileCertificate8").val(null);
        });

