        $("#btnSubmitCertificate").click(function()
        {
            var RegID=$("#hdnStuRegisID").val();
            var Count=0;
            var Title=[];
            var ProgramID=[];

            var photodata = new FormData();

            for (var i=0; i<10; i++)
            {
                var ID = "#Row" + [i+1];
                var CourseTitle="#txtCourseTitle"+[i+1];
                var ProgramName="#txtProgramName"+[i+1];
                var FileCertificate="#FileCertificate"+[i+1];
                var dim = $(ID).is(":visible");
                if (dim == true)
                {
                    if($(CourseTitle).val().trim()=="")
                    {
                        bootbox.alert("Please Enter Course Title for Row "+[i+1]);
                        return;
                    }
                    if($(ProgramName).val()==0||$(ProgramName).val()==null||$(ProgramName).val()==undefined||$(ProgramName).val()=="")
                    {
                        bootbox.alert("Please Select Program Name for Row "+[i+1]);
                        return;
                    }
                    if (RegID==0)
                    {
                        bootbox.alert("Please Refresh your Page or Login again.");
                        return;
                    }
                    else
                    {
                        var files = $(FileCertificate).get(0).files;
                        Title[i]=$(CourseTitle).val().trim();
                        ProgramID[i]=$(ProgramName).val();

                        if (files.length > 0)
                        {
                            FileNameIndividual = files[0]['name'];

                            FileExtensionIndividual = FileNameIndividual.split('.')[FileNameIndividual.split('.').length - 1];

                            var arrayExtensions = ["jpg", "jpeg", "JPEG", 'PDF', "JPG", "pdf","png","PNG"];
                            if (arrayExtensions.lastIndexOf(FileExtensionIndividual) == -1) {
                                bootbox.alert("Only JPG, JPEG, PDF file format is supported");
                                return;
                            }

                            photodata.append("photodata", files[0]);
                        }
                        else
                        {
                            bootbox.alert("Please Choose File for Row "+[i+1]);
                            return;
                        }
                        Count++;
                    }
                }

            }

            bootbox.confirm("Are You sure want to Upload?", function (result) {

                if (result == true) {

                    $.ajax({

                        datatype: "json",
                        type: "POST",
                        url: '../SMS/UploadCourseCertificates?RegID='+ RegID +'&CourseTitle=' + Title + '&ProgramName=' + ProgramID + '&Count=' + Count,

                        processData: false,
                        contentType: false,
                        data: photodata,
                        success: function (result) {

                            if (result==1)
                            {
                                bootbox.alert("Certificate Uploaded Succesfully");
                                FillGrid(37143);
                                ClearValues();

                            }
                            else if (result==2)                    {
                                bootbox.alert("Certificate Details Already Exist.");
                                return;
                            }
                            else
                            {
                                bootbox.alert("SomeThing went wrong..Please Contact to ERP-Cell. ");
                                return;
                            }
                        },
                        error: function (er) {
                            return;
                        }

                    });
                }
            });



        });
    });
    function ClearValues()
    {
        $("#txtCourseTitle1").val("");
        $("#txtCourseTitle2").val("");
        $("#txtCourseTitle3").val("");
        $("#txtCourseTitle4").val("");
        $("#txtCourseTitle5").val("");
        $("#txtCourseTitle6").val("");
        $("#txtCourseTitle7").val("");
        $("#txtCourseTitle8").val("");
        $("#txtCourseTitle9").val("");
        $("#txtCourseTitle10").val("");
        $("#txtProgramName1").val(0);
        $("#txtProgramName2").val(0);
        $("#txtProgramName3").val(0);
        $("#txtProgramName4").val(0);
        $("#txtProgramName5").val(0);
        $("#txtProgramName6").val(0);
        $("#txtProgramName7").val(0);
        $("#txtProgramName8").val(0);
        $("#txtProgramName9").val(0);
        $("#txtProgramName10").val(0);
        $("#FileCertificate1").val(null);
        $("#FileCertificate2").val(null);
        $("#FileCertificate3").val(null);
        $("#FileCertificate4").val(null);
        $("#FileCertificate5").val(null);
        $("#FileCertificate6").val(null);
        $("#FileCertificate7").val(null);
        $("#FileCertificate8").val(null);
        $("#FileCertificate9").val(null);
        $("#FileCertificate10").val(null);


    }

    function FillGrid(RegID)
    {

        $.ajaxSettings.traditional = true;

        $.ajax({
            url: '/SMS/GetStuCertificates',
            type: 'POST',
            datatype: 'json',
            data: {
                RegID: RegID
            },
            success: function (response) {

                $.jgrid.gridUnload('#StuCertificates');
                var dt = response.state;
                $("#StuCertificates").jqGrid({

                    datatype: "jsonstring",
                    mtype: 'GET',
                    width: null,
                    autowidth: false,
                    shrinkToFit: false,
                    height: "200",

                    colNames: [
                       'Delete',  'Preview','FID','Student ID','Extension','Course Title','Program Name','Name','Course','YearSem','Mobile No','Email-ID' ],

                    colModel: [
                                { name: 'Delete', index: 'Delete', width: 50, align: 'center', search: false, formatter: Delete, hidden: false },
                                { name: 'Preview', index: 'Preview', width: 50, align: 'center', editable: false, sortable: false, formatter: Preview },
                                { name: 'FID', index: 'FID', width: 50, align: 'center', search: true,hidden:true},
                                { name: 'StudentID', index: 'StudentID', width: 80, align: 'center', search: true,hidden:true},
                                { name: 'Extension', index: 'Extension', width: 80, align: 'center', search: true,hidden:true},
                                { name: 'CourseTitle', index: 'CourseTitle', width: 150, align: 'center', search: true},
                                { name: 'ProgramName', index: 'ProgramName', width: 100, align: 'center', search: true},
                                { name: 'Name', index: 'Name', width: 200, align: 'center', search: true},
                                { name: 'Course', index: 'Course', width: 220, align: 'left', search: true},
                                { name: 'YearSem', index: 'YearSem', width: 60, align: 'center', search: true},
                                { name: 'MobileNo', index: 'MobileNo', width: 100, align: 'center', search: true},
                                { name: 'EmailID', index: 'EmailID', width:200, align: 'left', search: true},
                    ],

                    datastr: dt,
                    rownumbers: true,
                    rowNum: 30,
                    viewrecords: true,
                    loadonce: true,
                    paging: true,
                    //multiselect: true,
                    align: "center",
                    pager: "#pgrCertificates",
                });

                $("#StuCertificates").jqGrid('filterToolbar', { stringResult: true, searchOnEnter: false, defaultSearch: "cn" });
            }
        });
    }
    function Delete(cellval, options, rowObject)
    {
        var FID = rowObject.FID;
        var Extension = rowObject.Extension;
        var button = "<input id=\"btnvw\" style= \" background-image:url('../Images/delete.png') ; background-size:cover; background-color:transparent; height:20px; width:20px; border :none;  \"  type=\"button\"  title=\"Select\"  onclick=\"javascript:Deletedata('" + FID + "','" + Extension + "')\" />";
        return button;
    }
    function Deletedata(FID,Extension)
    {
        bootbox.confirm("Are You sure want to Delete?", function (result) {

            if (result == true) {

                $.ajax({

                    url: '/SMS/DeleteSelectedCertificates',
                    datatype: "json",
                    type: "POST",
                    data: {

                        FID:FID,
                        Extension: Extension
                    },
                    success: function (result) {
                        if (result == 1) {
                            bootbox.alert("Information Deleted Successfully");
                            FillGrid(37143);
                            return;
                        }

                        else {
                            bootbox.alert("Information Can't Be deleted");
                            return;
                        }
                    }

                });

            }

        });

    }
    function Preview(cellValue, options, rowObject) {

        var FID = rowObject.FID;
        var Extension = rowObject.Extension;
        var button = "";
        if (Extension != "")
        {
            button = "<input id=\"btnvw\" style= \" background-image:url('../Images/Icons/search.png') ; background-size:cover; background-color:transparent; height:20px; width:20px; border :none;  \"  type=\"button\"  title=\"Select\"  onclick=\"javascript:PreviewData('" + FID + "','" + Extension + "')\" />";
        }
        else {
            button = "Not Uploaded";
        }
        return button;
    }

    function PreviewData(FID, Extension) {

        var dialog = bootbox.dialog({
            message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
            closeButton: false
        });

        $.ajax({
            url: '../SMS/ViewStudentCertificates',
            datatype: "json",
            type: 'POST',
            data: {
                FID: FID, Extension: Extension
            },
            success: function (response) {
                $("#PDFAnswer").attr('src', '');
                $('#ImageAnswer').prop('src', '');
                dialog.modal('hide');
                $("#ModalFileView").modal("show");

                var data = JSON.parse(response.state);

                if (data[0]["Attachment"] == "" || data[0]["Attachment"] == null || data[0]["Attachment"] == undefined) {
                    bootbox.alert("No Document Found");
                    return;
                }
                var Ext = $.trim(data[0]["Extension"]);

                if (Ext == ".pdf" || Ext == ".PDF") {
                    $("#divvPdf").show();
                    $("#divvImg").hide();
                    var dataURL = "data:application/pdf;base64," + data[0]['Attachment'];

                    $('#PDFAnswer').attr('src', dataURL);

                }
                if (Ext == ".jpg" || Ext == ".jpeg" || Ext == ".png" || Ext == ".bmp" || Ext == ".JPG" || Ext == ".JPEG" || Ext == ".PNG" || Ext == ".BMP") {

                    $("#divvPdf").hide();
                    $("#divvImg").show();
                    var dataURL = "data:image/jpeg;base64," + data[0]['Attachment'];
                    $('#ImageAnswer').prop('src', dataURL);

                }

            }
        });
    }
</script>

<script type="text/javascript">
    $(document).ready(function()
    {
        $("#btnSubmitAffidavit").click(function()
        {
            var RegID=$("#hdnStuRegisID").val();
            var YearSem=$("#txtSem").val();
            if (RegID==0)
            {
                bootbox.alert("Please Refresh your Page or Login again.");
                return;
            }

            $.ajaxSettings.traditional = true;
            $.ajax({
                type: "POST",
                url: '/SMS/InsertAffidavit',
                data:
                    {
                        RegID: RegID,YearSem:YearSem
                    },
                datatype: "json",
                success: function (result)
                {
                    if (result == 1)
                    {
                        $('#modalaffidavit').modal('hide');
                        bootbox.alert("Affdavit submit Successfully.");
                        return;

                    }
                    else
                    {
                        bootbox.alert("SomeThing went wrong. Please contact to ERP-Cell");
                    }
                }
            });

        });

    });
</script>

<script type="text/javascript">

    $(function(){

        $("#ddlCorrespondanceCountry").change(function() {
            var CountryID = ($("#ddlCorrespondanceCountry").val() == null || $("#ddlCorrespondanceCountry").val() == undefined || $("#ddlCorrespondanceCountry").val() == "") ? "0" : $("#ddlCorrespondanceCountry").val();
            FillState(CountryID, "0");
            $("#ddlCorrespondanceDistrict").empty();
            $("#ddlCorrespondanceCity").empty();
        });
        $("#ddlCorrespondanceState").change(function() {
            var StateID = ($("#ddlCorrespondanceState").val() == null || $("#ddlCorrespondanceState").val() == undefined || $("#ddlCorrespondanceState").val() == "") ? "0" : $("#ddlCorrespondanceState").val();
            FillDistrict(StateID, "0");
            $("#ddlCorrespondanceCity").empty();
        });
        $("#ddlCorrespondanceDistrict").change(function() {
            var DistrictID = ($("#ddlCorrespondanceDistrict").val() == null || $("#ddlCorrespondanceDistrict").val() == undefined || $("#ddlCorrespondanceDistrict").val() == "") ? "0" : $("#ddlCorrespondanceDistrict").val();
            FillCity(DistrictID, "0");
        });

        $("#btnUpdateCorrespondanceAddress").click(function() {

            var Address = $("#txtCorrespondanceAddress").val().trim();
            var PinCode = $("#txtCorrespondancePinCode").val().trim();
            var CountryID = ($("#ddlCorrespondanceCountry").val() == null || $("#ddlCorrespondanceCountry").val() == undefined || $("#ddlCorrespondanceCountry").val() == "") ? "0" : $("#ddlCorrespondanceCountry").val();
            var StateID = ($("#ddlCorrespondanceState").val() == null || $("#ddlCorrespondanceState").val() == undefined || $("#ddlCorrespondanceState").val() == "") ? "0" : $("#ddlCorrespondanceState").val();
            var DistrictID = ($("#ddlCorrespondanceDistrict").val() == null || $("#ddlCorrespondanceDistrict").val() == undefined || $("#ddlCorrespondanceDistrict").val() == "") ? "0" : $("#ddlCorrespondanceDistrict").val();
            var CityID = ($("#ddlCorrespondanceCity").val() == null || $("#ddlCorrespondanceCity").val() == undefined || $("#ddlCorrespondanceCity").val() == "") ? "0" : $("#ddlCorrespondanceCity").val();

            if (Address == ""){
                bootbox.alert("Please Enter Address.");
                return;
            }
            if (CountryID == "0"){
                bootbox.alert("Please Select Country.");
                return;
            }

            var params = [];
            params[0] = Address;
            params[1] = CountryID;
            params[2] = StateID;
            params[3] = DistrictID;
            params[4] = CityID;
            params[5] = PinCode;

            $.ajax({
                type: "POST",
                url: '../SMS/UpdateStudentDetailsOnStudentLogin',
                data: { param: params },
                datatype: "json",
                success: function (response) {
                    if (response.state == "1") {
                        bootbox.alert("Address Update successfully.");
                        return;
                    }
                    else
                    {
                        bootbox.alert("Some Thing Went Wrong, Please Contact To ERP Cell.");
                        return;
                    }
                }
            });

        });

    });

    function FillCountry(CountryID) {
        $.ajax({
            type: "POST",
            url: '../SMS/FillCountry',
            data: {},
            datatype: "json",
            success: function (response) {
                var dt = response;
                $("#ddlCorrespondanceCountry").empty();
                var ddlObj = $("#ddlCorrespondanceCountry");
                var items = ""; var selected = '';
                for (var i = 0; i < dt.length; i++) {
                    items += "<option value='" + dt[i].Value + "'>" + dt[i].Text + "</option>";
                }
                ddlObj.html(items).show();
                ddlObj.val(CountryID);
            }
        });
    }

    function FillState(CountryID, StateID) {

        $.ajax({
            type: "POST",
            url: '../SMS/FillState',
            data: { CountryID: CountryID },
            datatype: "json",
            success: function (response) {
                var dt = response;
                $("#ddlCorrespondanceState").empty();
                var ddlObj = $("#ddlCorrespondanceState");
                var items = "";
                for (var i = 0; i < dt.length; i++) {
                    items += "<option  value='" + dt[i].Value + "'>" + dt[i].Text + "</option>";
                }
                ddlObj.html(items).show();
                ddlObj.val(StateID);
            }
        });
    }

    function FillDistrict(StateID, DistrictID) {

        $.ajax({
            type: "POST",
            url: '../HRM/FillDistrict',
            data: { StateID: StateID },
            datatype: "json",
            success: function (response) {
                var dt = response;
                $("#ddlCorrespondanceDistrict").empty();
                var ddlObj = $("#ddlCorrespondanceDistrict");
                var items = "";
                for (var i = 0; i < dt.length; i++) {
                    items += "<option selected=\"selected\" value='" + dt[i].Value + "'>" + dt[i].Text + "</option>";
                }
                ddlObj.html(items).show();
                ddlObj.val(DistrictID);
            }
        });
    }

    function FillCity(DistrictID, CityID) {
        $.ajax({
            type: "POST",
            url: '../HRM/FillCity',
            data: { districtID: DistrictID },
            datatype: "json",
            success: function (response) {
                var dt = response;
                $("#ddlCorrespondanceCity").empty();
                var ddlObj = $("#ddlCorrespondanceCity");
                var items = "";
                for (var i = 0; i < dt.length; i++) {
                    items += "<option selected=\"selected\" value='" + dt[i].Value + "'>" + dt[i].Text + "</option>";
                }
                ddlObj.html(items).show();
                ddlObj.val(CityID);
            }
        });
    }

</script>

<script type="text/javascript">
    $(document).ready(function () {                 ///////////////////Placement//////////////////////

        $(".divremarks").hide();
        $(".divReasons").hide();

        $("#ddlApproval").change(function () {

            if ($("#ddlApproval").val() == 0 || $("#ddlApproval").val() == 1)  {

                $(".divremarks").hide();
                $(".divReasons").hide();

            }
            else
            {

                $(".divReasons").show();
            }
        })


        $("#ddlReasons").change(function () {

            if ($("#ddlReasons").val() == 5 )  {

                $(".divremarks").show();

            }
            else
            {
                $(".divremarks").hide();

            }
        })


        $("#btnsubmitPlacement").click(function () {

            var fileExtension="";
            var txtInternshipDetailstpa="";
            var txtDurationtpa="";
            var txtProjectTitletpa="";


            if ($("#ddlApproval").val() == 1) {

                if ($("#txtContactNotpa").val() == "0") {
                    bootbox.alert("Please Enter Contact No .");
                    return;
                }
                if ($("#txtEmailIDtpa").val() == "") {
                    bootbox.alert("Please Enter Email ID");

                    return;
                }


                if ($("#FileResumePlacementtpa").val().trim() == "") {
                    bootbox.alert("Please Upload Resume");
                    return;
                }


                var docFile = $("#FileResumePlacementtpa").get(0).files;
                var Document = new FormData();

                if (docFile.length > 0) {

                    var fileExtension = "";
                    var fileName = docFile[0]['name'];
                    fileExtension = fileName.substr((fileName.lastIndexOf('.')));

                    if('UU' == 'QGC'){

                        var arrayExtensions = ['.PDF',  ".pdf"];
                        if (arrayExtensions.lastIndexOf(fileExtension) == -1) {
                            bootbox.alert("Only  PDF file formats are supported.");
                            return;
                        }
                    }
                    else
                    {
                        var arrayExtensions = ['.PDF',  ".pdf", '.doc', '.docx'];
                        if (arrayExtensions.lastIndexOf(fileExtension) == -1) {
                            bootbox.alert("Only Word and PDF file formats are supported.");
                            return;
                        }
                    }
                    Document.append("helpSectionImages", docFile[0]);

                    if (docFile[0].size > 614400) {
                        bootbox.alert('Document Size should be less than 600KB.');
                        return;
                    }
                }
            }
            if ($("#ddlApproval").val().trim() == 0) {
                bootbox.alert("Please select Placement Support required");//
                return;
            }


            if ($("#ddlApproval").val() == 2) {

                if ($("#ddlReasons").val() == 0) {
                    bootbox.alert("Please Select One Reason");
                    return;
                }
            }

            if ($("#ddlReasons").val() == 5) {

                if ($("#txtRemarkstpa").val() == "") {
                    bootbox.alert("Please Enter Remarks");
                    return;
                }
            }


            bootbox.confirm("Are you sure you want to save", function (result) {

                if (result == true) {
                    $.ajaxSettings.traditional = true;

                    var param = [];

                    param[0] = $("#txtContactNotpa").val();
                    param[1] = $("#txtEmailIDtpa").val();
                    param[2] = $("#txtInternshipDetailstpa").val();
                    param[3] = $("#txtDurationtpa").val();
                    param[4] = $("#txtProjectTitletpa").val();
                    param[5] = $("#ddlReasons").val();
                    param[6] = $("#ddlApproval").val();
                    param[7] = $("#txtRemarkstpa").val();


                    $.ajax({

                        url: '/Account/RegisterStudentDetailsPlacement',
                        datatype: "json",
                        type: 'POST',
                        data: {
                            param: param, fileExtension: fileExtension
                        },
                        success: function (result) {

                            if (result == "1") {

                                InsertAttachment(result, Document, fileExtension);
                                $("#StudentInfoplacement").modal('hide');
                                bootbox.alert("Data save successfully");
                                //ClearValue();
                                return;
                            }
                            else {
                                bootbox.alert("Something Went wrong.");
                                return;
                            }
                        }

                    })
                }
            })

        })

    });


    function InsertAttachment(ID, photodata, fileExtension) {

        $.ajaxSettings.traditional = true;

        $.ajax({
            url: '../Web_StudentAcademic/InsertResumeImage?ID=' + ID + '&fileExtension=' + fileExtension,
            datatype: "json",
            type: 'POST',
            processData: false,
            contentType: false,
            data: photodata,
            async: true,
            success: function (response) {

            }
        });
    }

</script>

<script type="text/javascript">

    $(document).ready(function () {

        FillSpecialization();
        
        $("#btnsubmitSpecialization").click(function () {

            var RegID=$("#hdnStuRegisID").val();

            var SpecializationID=$("#ddlSpecialization").val();

            if (RegID==0)
            {
                bootbox.alert("Please Refresh your Page or Login again.");
                return;
            }

            bootbox.confirm("Are you sure you want to submit Specialization", function (result) {
                if (result) {

                    $.ajaxSettings.traditional = true;

                    $.ajax({
                      url: '/SMS/InsertStudentSpecialization',
                        datatype: "json",
                        type: 'POST',
                        data:
                        {
                            RegID: RegID,SpecializationID:SpecializationID
                        },
                        success: function (result) {

                            if (result == 1) {
                                SuccessMsg("Specialization submit Successfully.");
                                $('#StudentSpecialization').modal('hide');
                                return;
                            }
                            else {
                                FailedMsg("Oops Something Went's Wrong. Please try Again!!!")
                                return;
                            }
                        }
                    })
                }
            });


        });


    });
</script>

<script type="text/javascript">


    function FillSpecialization() {

        $.ajax({
            type: "POST",
            url: '/SMS/FillSpecialization',
            data: {
            },
            datatype: "json",
            success: function (response) {
                var dt = response;

                $("#ddlSpecialization").empty();
                var ddlObj = $("#ddlSpecialization");
                var items = ""; var curr = "";
                for (var i = 0; i < dt.length; i++) {
                    items += "<option selected=\"selected\" value='" + dt[i].Value + "'>" + dt[i].Text + "</option>";
                    if (i == 0) {
                        curr = dt[i].Value;
                    }
                }
                ddlObj.html(items).show();
                ddlObj.val(curr);
            }
        });
    }



</script>

