    <script>
        function openNav() {
            document.getElementById("mySidenavv").style.width = "200px";
        }

        function closeNav() {
            document.getElementById("mySidenavv").style.width = "0";
        }



        function redirectFee() {
            var config = "UU";
            if (config == "GEU" || config == "UU" || config == "QGC") {
                window.location.href = '../Web_StudentFinance/Cyborg_StudentOnlineFeeSubmission?id=Fee%20Submission';
            } else {
                alert('Not permitted');
                return;
            }
        }
    </script>





