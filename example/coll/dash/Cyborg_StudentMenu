<script src="/ProjectJS_Org/CommonMessages.js"></script>

<script type="text/javascript">
    var dt;
    var count = 0;
    var length = 0;
    $(document).ready(function(){

        $("#chkFinalUpdate").change(function() {
            toggleButtons();
        });

        // Function to toggle the buttons based on the checkbox state
        function toggleButtons() {
            if ($("#chkFinalUpdate").prop('checked')) {  // If checked

                //$("#finalsavemobileemail").show();
                //$("#btnSubmitEmailMobileNo").hide();
                $("#btnSubmitEmailMobileNo").text("Update");

                $("#txtStuEmail").prop('disabled', false);
                $("#txtStuMobile").prop('disabled', false);
            } else {  // If unchecked
                $("#btnSubmitEmailMobileNo").text("Save");
                $("#txtStuEmail").prop('disabled', true);
                $("#txtStuMobile").prop('disabled', true);
            }
        }
       

        $('#btnnext').click(function () {
            var circularData = dt;
            for (var i = count; i < circularData.length; i++) {
                var ShowAsAPopup = circularData[i]["ShowAsAPopup"];
                if (ShowAsAPopup == true) {
                    $("#modelShowAsAPopup").modal('show');
                    var ImageExtension = circularData[i]["ImageExtension"];
                    var TextValue =circularData[i]["Notice"];
                    if (TextValue !=null && TextValue !="") {
                        $("#divPdf").hide();
                        $("#divImg").hide();
                        $("#docImg").hide();
                        $("#TextNotification").show();
                        $("#NotificationTextValue").html('');
                        $("#NotificationTextValue").html(TextValue);
                    }
                    if (ImageExtension == ".pdf" || ImageExtension == ".PDF") {
                        $("#divPdf").show();
                        $("#divImg").hide();
                        $("#docImg").hide();
                        $("#NotificationTextValue").html('');
                        $("#NotificationTextValue").html(TextValue);
                        var dataURL = "data:application/pdf;base64," + circularData[i]['Circular'];
                        $('#ImgNotice').attr('src', dataURL);
                        $("#cir_subID").html(circularData[i]["Subject"]);
                        $("#ImgNotice").prop('type', 'application/pdf');
                    }
                    else if (ImageExtension == ".jpg" || ImageExtension == ".jpeg" || ImageExtension == ".png" || ImageExtension == ".bmp" ||
                        ImageExtension == ".JPG" || ImageExtension == ".JPEG" || ImageExtension == ".PNG" || ImageExtension == ".BMP") {
                        $("#divPdf").hide();
                        $("#divImg").show();
                        $("#docImg").hide();
                        $("#NotificationTextValue").html('');
                        $("#NotificationTextValue").html(TextValue);
                        var dataURL = "data:image/jpeg;base64," + circularData[i]['Circular'];
                        $('#imgNoticeImg').prop('src', dataURL);
                        $("#cir_subID").html(circularData[i]["Subject"]);
                    }
                    else if (ImageExtension == ".docx" || ImageExtension == ".DOCX") {
                        $("#divPdf").hide();
                        $("#divImg").hide();
                        $("#NotificationTextValue").html('');
                        $("#NotificationTextValue").html(TextValue);
                        var dataURL = "data:application/vnd.openxmlformats-officedocument.wordprocessingml.document;base64," + circularData[0]['Circular'];
                        $("#docLink").attr("href", '');
                        $("#docLink").attr("href", dataURL);
                        $("#cir_subID").html("");
                        $("#cir_subID").html(circularData[i]["Subject"]);
                    }
                    else if (ImageExtension == ".doc" || ImageExtension == ".DOC") {
                        $('#imgNoticeImg').prop('src', '');
                        $("#divPdf").hide();
                        $("#divImg").hide();
                        $("#NotificationTextValue").html('');
                        $("#NotificationTextValue").html(TextValue);
                        var dataURL = "data:application/msword;base64," + circularData[i]['Circular'];
                        $("#docLink").attr("href", dataURL);
                        $("#cir_subID").html("");
                        $("#cir_subID").html(circularData[i]["Subject"]);
                    }
                    count = count + 1;
                    break;
                }
            }
        });
    });


    function FillmailAndNumber(dtEmailAndNumber) {

        var dt = JSON.parse(dtEmailAndNumber);

        //if(dtEmailAndNumber.length > 0){
        //    $("#btnSave").html('Update');
        //}
        $("#txtStuEmail").val(dt[0]["PEMAIL"]);
        $("#txtStuMobile").val(dt[0]["PPHMobile"]);
        if(dt[0]["isMobileEmailUpdate"] == 0){
           $("#modalVerificatioMobileNoAndEmail").modal('show');
        }
    }


    function GetOnlineExamHome() {

        $.ajaxSettings.traditional = true;
        //href='OnlineExam/Cyborg_OnlineExamlist'
        $.ajax({
            url: '/Account/GetOnlineExamHome',
            datatype: "json",
            type: 'POST',
            data: {},
            success: function (response) {
                var dtMsg = JSON.parse(response);
                $("#tblOnlineExam").append("<tbody width=\"30%\">");
                $("#tblOnlineExamView").append("<tbody width=\"30%\">");
                for (var i = 0; i < dtMsg.length; i++) {
                    if(dtMsg[i]["ExType"] == '1') {
                        $("#tblOnlineExam").append("<tr width=\"30%\"><td  width=30%;><p style=\"color:#878787;\"><b>" + dtMsg[i]["ExamDate"] + "&nbsp; &nbsp; </b></p ><a  href=\"#\" onclick=redirectExam("+dtMsg[i]["ExType"]+","+dtMsg[i]["IsMCQ"]+")><b style=\"font-size:10px;\">" + dtMsg[i]["ExamName"] + " </a></td></tr>");
                        $("#tblOnlineExamView").append("<tr width=\"30%\"><td  width=30%;><p style=\"color:#878787;\"><b>" + dtMsg[i]["ExamDate"] + "&nbsp; &nbsp; </b></p ><a href=\"#\"  onclick=redirectExam("+dtMsg[i]["ExType"]+","+dtMsg[i]["IsMCQ"]+")><b style=\"font-size:10px;\">" + dtMsg[i]["ExamName"] + " </a></td></tr>");
                    } else {
                        $("#tblOnlineExam").append("<tr width=\"30%\"><td  width=30%;><p style=\"color:#878787;\"><b>" + dtMsg[i]["ExamDate"] + "&nbsp; &nbsp; </b></p ><a href=\"#\" onclick=redirectExam("+dtMsg[i]["ExType"]+","+dtMsg[i]["IsMCQ"]+")><b style=\"font-size:10px;\">" + dtMsg[i]["ExamName"] + " </a></td></tr>");
                        $("#tblOnlineExamView").append("<tr width=\"30%\"><td  width=30%;><p style=\"color:#878787;\"><b>" + dtMsg[i]["ExamDate"] + "&nbsp; &nbsp; </b></p ><a href=\"#\" onclick=redirectExam("+dtMsg[i]["ExType"]+","+dtMsg[i]["IsMCQ"]+")><b style=\"font-size:10px;\">" + dtMsg[i]["ExamName"] + " </a></td></tr>");
                    }
                }

                if (dtMsg.length > 0) {
                    $("#btnMockTest").hide();
                    $("#dvinstructionDescriptive").hide();
                    $("#dvinstructionSessional").show();

                    if ('UU' != 'UU')
                    {
                        $("#modelOnlineExam").modal('show');
                    }
                    else
                    {
                        $("#modelOnlineExam").modal('show');
                    }

                }

                if (('UU' == 'DOON') && dtMsg.length > 0) {
                    $("#btnMockTest").hide();
                    $("#dvinstructionDescriptive").hide();
                    $("#dvinstructionSessional").hide();
                    $("#modelOnlineExam").modal('show');
                }

                if (('UU' == 'BFIT') && dtMsg.length > 0){
                    $("#modelOnlineExam").modal('hide');
                }



                if (('UU' == 'UU') && dtMsg.length > 0)
                {
                    // if (dtMsg[0]["IsFinalYear"] == '1') {
                    $("#btnMockTest").hide();
                    $("#tdinstructionTitle").text('GENERAL INSTRUCTIONS');
                    $("#dvinstructionDescriptive").hide();
                    $("#dvinstructionSessional").show();

                    //}
                    // else {
                    //     $("#btnMockTest").show();
                    //    $("#tdinstructionTitle").text('Instruction for Online Sessional Examination');
                    //    $("#dvinstructionDescriptive").hide();
                    //    $("#dvinstructionSessional").show();
                    //}
                    if ('UU' != 'UU')
                    {
                        $("#modelOnlineExam").modal('show');
                    }
                    else
                    {
                        $("#modelOnlineExam").modal('show');
                    }

                }
            }
        });
    }


    function redirectExam(ExamType,IsMCQ)
    {
        if ('UU' != 'UU'){

            if (ExamType == '1') {
                window.location.href = '../OnlineExam/Cyborg_OnlineExamlist';
            }
            else
            {
                window.location.href = '../Web_Exam/Cyborg_OnlineExamStudentSubject';
            }
        }
        else if ('UU' == 'UU'){

            if (ExamType == '1' && IsMCQ == 1) {
                //window.location.href = '../OnlineExam/Cyborg_OnlineExamlist';
            }
            else
            {
                //window.location.href = '../Web_Exam/Cyborg_OnlineExamStudentSubject';
            }
        }

    }

    function EventCreation(events) {
        var dtEvents = JSON.parse(events);

        $("#tblEvents").append("<tbody>");
        for (var i = 0; i < dtEvents.length; i++) {

            //$("#tblEvents").append("<tr width=\"100%\" class=\"success\"><td></td width=20%;><td width=80%;><a target=\"_blank\" href=../Acedmic/Cyborg_EventDescription?EventID=" + dtEvents[i]["EventId"]+ " >" + dtEvents[i]["Event"] + " </a></td></tr>");
            if ((i % 2) == 0) {
                $("#tblEvents").append("<tr width=\"100%\"><td width=80%;><a target=\"_blank\" href=../Media/Cyborg_EventDescription?EventID=" + dtEvents[i]["EventId"] + " >" + dtEvents[i]["Event"] + " </a></td><td width=20%;>" + dtEvents[i]["Date"] + "</td ></tr>");
                i = i + 1
            }
            else {
                $("#tblEvents").append("<tr width=\"100%\" ><td width=80%;><a target=\"_blank\" href=../Media/Cyborg_EventDescription?EventID=" + dtEvents[i]["EventId"] + " >" + dtEvents[i]["Event"] + " </a></td><td width=20%;>" + dtEvents[i]["Date"] + "</td></tr>");
                i = i + 1
            }
        }
    }

    function FillNews(dt) {

        var dtNews = JSON.parse(dt);
        for (var i = 0; i < dtNews.length; i++) {

            var img = "data:image/jpeg;base64," + dtNews[i]['Clip'];

            if (dtNews[i]["Heading"] == "") {
                
                $("#tblNews").append("<tr  valign='top'> <td style=\"width:20%;\"><img style=\"width:60px;height: 40px\" id=\"" + dtNews[i]["ClipID"] + "\" runat=\"server\" src= \"" + img + "\"/> </td> <td> <table> <tr valign='top'> <td style=\"font-size:16px;\"><a href=\"../Media/Cyborg_NewsView?clipID=" + dtNews[i]["ClipID"] + "\" target=\"_blank\" style=\"font-family:'Kruti Dev 010';font-size:16px;\">" + dtNews[i]["HindiHeading"] + "</a></td></tr> <tr valign='top'> <td style=\"font-size:13px;color:#F36858;\"> " + dtNews[i]["PublishDate"] + "</td></tr><tr valign='top'> <td style=\"font-size:11px;color:#787878;\"> " + dtNews[i]["Source"] + "</td></tr></table></td></tr>");
            }
            else {
                $("#tblNews").append("<tr  valign='top'> <td width=20%;><img style=\"width:60px;height: 40px\" id=\"" + dtNews[i]["ClipID"] + "\" runat=\"server\" src= \"" + img + "\"/> </td> <td width=80%;> <table> <tr valign='top'> <td style=\"font-size:16px;\"><a href=\"\"../Media/Cyborg_NewsView?clipID=" + dtNews[i]["ClipID"] + "\" target=\"_blank\" style=\"font-size:16px;\">" + dtNews[i]["Heading"] + "</a></td></tr> <tr valign='top'> <td style=\"font-size:13px;color:#F36858;\"> " + dtNews[i]["PublishDate"] + "</td></tr><tr valign='top'> <td style=\"font-size:11px; color:#787878;\"> " + dtNews[i]["Source"] + "</td></tr></table></td></tr><br> ");

            }
        }
    }


    function FillSubstitute(dt) {


        var dtSubstitute = JSON.parse(dt);
        for (var i = 0; i < dtSubstitute.length; i++) {

            $("#tblSubstitute").append("<tr  valign='top'> <td width=30%;> <table> <tr valign='top'> <td style=\"font-size:10px;color:#F36858;\">" + dtSubstitute[i]["Duration"] + " </td> </tr>  <tr valign='top'> <td style=\"font-size:10px;color:#F36858;\">Period : " + dtSubstitute[i]["PrePeriod"] + " </td> </tr></table>  </td> <td width=70%;> <table> <tr valign='top'> <td style=\"font-size:16px;\"><a onclick=\"FillSubstituteInfo('" + dtSubstitute[i]["PreSubject"] + "','" + dtSubstitute[i]["PreEmployee"] + "','" + dtSubstitute[i]["PrePeriod"] + "','" + dtSubstitute[i]["subject"] + "','" + dtSubstitute[i]["Employee"] + "','" + dtSubstitute[i]["Duration"] + "','" + dtSubstitute[i]["SubstituteDate"] + "')\" style=\"font-size:10px;\">" + dtSubstitute[i]["subject"] + "</a></td></tr> <tr valign='top'> <td style=\"font-size:10px;color:#F36858;\"> " + dtSubstitute[i]["Employee"] + "</td></tr></table></td></tr><br> ");
            //$("#tblSubstitute").append("<tr valign='top'> <td width=25%; style=\"width:60px;height: 40px;font-size:10px;color:#F36858;\"> " + dtSubstitute[i]["Duration"] + " </td> <td width=75%;> <table> <tr valign='top'> <td style=\"font-size:16px;\"><a onclick=\"FillSubstituteInfo('" + dtSubstitute[i]["PreSubject"] + "','" + dtSubstitute[i]["PreEmployee"] + "','" + dtSubstitute[i]["PrePeriod"] + "','" + dtSubstitute[i]["subject"] + "','" + dtSubstitute[i]["Employee"] + "','" + dtSubstitute[i]["Duration"] + "','" + dtSubstitute[i]["SubstituteDate"] + "')\" style=\"font-size:10px;\">" + dtSubstitute[i]["subject"] + "</a></td></tr> <tr valign='top'> <td style=\"font-size:10px;color:#F36858;\"> " + dtSubstitute[i]["Employee"] + "</td></tr></table></td></tr><br> ");
        }


    }

    function FillSubstituteInfo(PreSubject, PreEmployee, PrePeriod, subject, Employee, Duration, SubstituteDate) {

        $("#SubsPeriod").text(PrePeriod);
        $("#SubsDate").text(SubstituteDate);
        $("#SubsPreSubject").text(PreSubject);
        $("#SubsPreEmployee").text(PreEmployee);
        $("#SubsSubject").text(subject);
        $("#SubsEmployee").text(Employee);
        $("#SubsTime").text(Duration);
        $("#modelSubjectSubstitute").modal('show');

    }

    function FillCircularNotices() {
        $.ajax({

            url: '/Account/GetCircularIntention',
            datatype: "json",
            type: 'POST',
            data: {},
            success: function (response) {
                var dtEmailAndNumber = response.dtEmailAndNumber;
                var circular = response.circular;
                var events = response.events;
                var news = response.news;
                var Substitute = response.Substitute;
                var dtManual = response.dtManual;
                

                FillNews(news);
                EventCreation(events);
                FillSubstitute(Substitute);
                FillmailAndNumber(dtEmailAndNumber);

                var inHTML = "";
                var circularData = JSON.parse(circular);
                dt = circularData;
                for (var i = 0; i < circularData.length; i++) {
                    //var EmpCircular = GetCirID(circularData[i]["CirID"]);
                    //var cirid = (circularData[i]["chk"]);

                    //if (cirid == 1) {
                    //    $("#tblCircular").append("<tr width=\"100%\"><td style=\"color:#878787;\" width=100%;><p><b>" + circularData[i]["CreatedOn"] + "</b></p><a target=\"_blank\" href=../Web_StudentAcademic/Cyborg_StudentCircularView?CirID=" + circularData[i]["CirID"] + "  style=\"color:blue\"  >" + circularData[i]["Subject"] + " </a></td></tr>");
                    //}
                    //else {
                    //    $("#tblCircular").append("<tr width=\"100%\"><td style=\"color:#878787;\" width=100%;><p><b>" + circularData[i]["CreatedOn"] + "</b></p><a target=\"_blank\" href=../Web_StudentAcademic/Cyborg_StudentCircularView?CirID=" + circularData[i]["CirID"] + " style=\"color:red\" onclick=\"javascript:InsertCir('" + circularData[i]["CirID"] + "')\">" + circularData[i]["Subject"] + " </a></td></tr>");
                    //}

                    $("#tblCircular").append("<tr width=\"100%\"><td style=\"color:#878787;\" width=100%;><p><b>" + circularData[i]["CreatedOn"] + "</b></p><a target=\"_blank\" href=../Web_StudentAcademic/Cyborg_StudentCircularView?CirID=" + circularData[i]["CirID"] + "&Type="+ 1 +"  style=\"color:blue\"  >" + circularData[i]["Subject"] + " </a></td></tr>");

                }

                var Manualdata = JSON.parse(dtManual);
                for (var i = 0; i < Manualdata.length; i++) {

                    $("#tblManual").append("<tr width=\"100%\"><td style=\"color:#878787;\" width=100%;><p><b>" + Manualdata[i]["ModifiedDate"] + "</b></p><a target=\"_blank\" href=../Web_StudentAcademic/Cyborg_StudentManualView?CirID=" + Manualdata[i]["MID"] + "&Type="+ 1 +"  style=\"color:blue\"  >" + Manualdata[i]["Subject"] + " </a></td></tr>");
                }

                for (var i = 0; i < circularData.length; i++) {

                    var ShowAsAPopup = circularData[i]["ShowAsAPopup"];


                    if (ShowAsAPopup == "1") {
                        $("#modelShowAsAPopup").modal('show');


                        var ImageExtension = circularData[i]["ImageExtension"];
                        var TextValue =circularData[i]["Notice"];
                        if (TextValue !=null && TextValue !="") {
                            $("#divPdf").hide();
                            $("#divImg").hide();
                            $("#docImg").hide();
                            $("#TextNotification").show();
                            $("#NotificationTextValue").html('');
                            $("#NotificationTextValue").html(TextValue);
                        }
                        else if (ImageExtension == ".pdf" || ImageExtension == ".PDF") {
                            $("#divPdf").show();
                            $("#divImg").hide();
                            $("#docImg").hide();
                            $("#NotificationTextValue").html('');
                            $("#TextNotification").hide();
                            var dataURL = "data:application/pdf;base64," + circularData[i]['Circular'];
                            $("#ImgNotice").attr("src", '');
                            $('#ImgNotice').attr('src', dataURL);
                            $("#cir_subID").html(circularData[i]["Subject"]);
                            $("#ImgNotice").prop('type', 'application/pdf');
                        }
                        else if (ImageExtension == ".jpg" || ImageExtension == ".jpeg" || ImageExtension == ".png" || ImageExtension == ".bmp" ||
                            ImageExtension == ".JPG" || ImageExtension == ".JPEG" || ImageExtension == ".PNG" || ImageExtension == ".BMP") {
                            $("#divPdf").hide();
                            $("#divImg").show();
                            $("#docImg").hide();
                            $("#NotificationTextValue").html('');
                            $("#TextNotification").hide();

                            var dataURL = "data:image/jpeg;base64," + circularData[i]['Circular'];
                            $('#imgNoticeImg').prop('src', dataURL);
                            $("#cir_subID").html(circularData[i]["Subject"]);
                        }
                        else if (ImageExtension == ".docx" || ImageExtension == ".DOCX") {
                            $("#divPdf").hide();
                            $("#divImg").hide();
                            $("#NotificationTextValue").html('');
                            $("#TextNotification").hide();
                            var dataURL = "data:application/vnd.openxmlformats-officedocument.wordprocessingml.document;base64," + circularData[0]['Circular'];
                            $("#docLink").attr("href", dataURL);
                            $("#cir_subID").html("");
                            $("#cir_subID").html(circularData[i]["Subject"]);
                        }
                        else if (ImageExtension == ".doc" || ImageExtension == ".DOC") {
                            $("#divPdf").hide();
                            $("#divImg").hide();
                            $("#NotificationTextValue").html('');
                            $("#TextNotification").hide();
                            var dataURL = "data:application/msword;base64," + circularData[i]['Circular'];
                            $("#docLink").attr("href", dataURL);
                            $("#cir_subID").html("");
                            $("#cir_subID").html(circularData[i]["Subject"]);
                        }
                        count = count + 1;
                        break;

                    }

                }
            }
        });
    }

    function FillResults() {
        $.ajax({

            url: '/Account/GetResultIntension',
            datatype: "json",
            type: 'POST',
            data: {},
            success: function (response) {

                var result = response.result;

                var inHTML = "";
                var resultdata = JSON.parse(result);
                for (var i = 0; i < resultdata.length; i++) {

                    var cirid = (resultdata[i]["CirID"]).toString();
                    var uid = '37143';
                    var usertype = '2';
                    var Courseid = (resultdata[i]["CourseID"]).toString();
                    var EvalType = (resultdata[i]["EvalType"]).toString();

                    var config = "UU";
                    if (config == "UU") {

                        if (Courseid == "27" || Courseid == "59") {
                            //$("#tblResult").append("<tr width=\"100%\" ><td width=100%;><a target=\"_blank\" href=\"http://app.cyborgit.in?at=" + uid + "&atyp=" + usertype + "\"><b>" + resultdata[i]["Subject"] + "</b></a></td></tr>");
                            $("#tblResult").append('<tr width="100%" ><td width=100%;><a  onclick="showstudentresultFor27_59CourseIDs(' + cirid+ ','+uid+','+usertype+')" href="#"><b>' + resultdata[i]["Subject"] + '</b></a></td></tr>');
                        }
                        else {
                            if (EvalType == '0') {
                                $("#tblResult").append("<tr width=\"100%\" ><td width=100%;><a target=\"_blank\" href=\"http://app.cyborgit.in?at=" + uid + "&atyp=" + usertype + "\"><b>" + resultdata[i]["Subject"] + "</b></a></td></tr>");
                            }
                            else {
                                $("#tblResult").append('<tr width="100%" ><td width=100%;><a  onclick="showstudentresult(' + resultdata[i]["YearSem"] + ')" href="#"><b>' + resultdata[i]["Subject"] + '</b></a></td></tr>');
                            }
                        }
                    }
                    else {
                        $("#tblResult").append('<tr width="100%" ><td width=100%;><a  onclick="showstudentresult(' + resultdata[i]["YearSem"] + ')" href="#"><b>' + resultdata[i]["Subject"] + '</b></a></td></tr>');
                    }
                }
            }
        });
    }

    function showstudentresultFor27_59CourseIDs(cirid,uid,usertype){

        window.open("../CExms/Cyborg_StudentResultViewMarkBase?CirID="+cirid +"&uid="+uid+"&usertype="+ usertype);
    }

    function showstudentresult(YearSem) {
        var uid = '37143';
        $.ajax({
            url: '/Web_StudentAcademic/FillMarksheet',
            datatype: "json",
            type: 'POST',
            data: { yearSem: YearSem, Regid: uid },

            success: function (response) {
                $("#dvLoading").hide();
                if (response.msg == "OK") {
                    if (response.data == 1) {
                        window.open('../SMSReport/Cyborg_SMSCommonViewer', '_blank');

                    } else {
                        window.open('../Web_StudentAcademic/Cyborg_ViewrdlcWithTwodataset', '_blank');
                    }
                } else {
                    bootbox.alert(response.msg);
                }

            }
        });
    }



    function FillUpcomingCompany() {
        $.ajaxSettings.traditional = true;

        $.ajax({

            url: '/Web_StudentAcademic/GetUpcomingCompany',
            datatype: "json",
            type: 'POST',

            data: { RegID: "37143" },
            success: function (response) {
                var dtTNP = JSON.parse(response.state);

                $("#tblTNP").append("<tbody>");
                for (var i = 0; i < dtTNP.length; i++) {


                    if('UU' == 'UU' && (dtTNP[i]["StuPlacment"] == "1") && (dtTNP[i]["StuCampus"] == "0")&& (dtTNP[i]["CampusCourseID"] == "1"))//
                    {

                        $("#tblTNP").append("<tr width=\"100%\"><td width=80%;><span>Campus Drive<span>  <a target=\"_blank\" href=../TPA/Cyborg_StudentCampusDrive?CVID=" + dtTNP[i]["CVID"] + "><b style=\"font-size:12px;\">" + dtTNP[i]["CompanyName"] + "</td ></tr>");

                    }
                    else if  ('UU' == 'QGC' && (dtTNP[i]["StuPlacment"] == "1") && (dtTNP[i]["StuCampus"] == "0")&& (dtTNP[i]["CampusCourseID"] == "1"))
                    {

                        $("#tblTNP").append("<tr width=\"100%\"><td width=80%;><span>Campus Drive<span>  <a target=\"_blank\" href=../TPA/Cyborg_StudentCampusDrive?CVID=" + dtTNP[i]["CVID"] + "><b style=\"font-size:12px;\">" + dtTNP[i]["CompanyName"] + "</td ></tr>");
                    }

                    else {
                        $("#tblTNP").append("<tr width=\"100%\"><td width=80%;><span> Campus Drive<span> <a target=\"_blank\" ><b style=\"font-size:12px;\">" + dtTNP[i]["CompanyName"] + "</td ></tr>");

                    }

                }
            }
        });
    }

    function FillNotification() {
        $.ajaxSettings.traditional = true;

        $.ajax({

            url: '/Account/GetStudentMsgHistory',
            datatype: "json",
            type: 'POST',

            data: {},
            success: function (response) {
                var dtMsg = JSON.parse(response.result);

                $("#tblNotifi").append("<tbody width=\"30%\">");
                for (var i = 0; i < dtMsg.length; i++) {

                    //$("#tblEvents").append("<tr width=\"100%\" class=\"success\"><td></td width=20%;><td width=80%;><a target=\"_blank\" href=../Acedmic/Cyborg_EventDescription?EventID=" + dtEvents[i]["EventId"]+ " >" + dtEvents[i]["Event"] + " </a></td></tr>");

                    $("#tblNotifi").append("<tr width=\"30%\"><td  width=30%;><p style=\"color:#878787;\"><b>" + dtMsg[i]["Date"] + "</b></p ><p target=\"_blank\" href=#?CVID=" + dtMsg[i]["msgID"] + " ><b style=\"font-size:10px;\">" + dtMsg[i]["MESSAGE"] + " </p></td></tr>");


                }
            }
        });
    }

    function InsertCir(CirID) {
        var cirid = CirID.toString();
        var EmpCircular = GetCirID(CirID);
        if (EmpCircular.indexOf(cirid) !== -1) {
            //localStorage.setItem('myMainKey', CirID);
            //window.open("../Acedmic/Cyborg_CircularDetail?CirID=" + CirID);

        }
        else {
            $.ajax({

                url: '/Media/InsertCirID',
                datatype: "json",
                type: 'POST',
                async: false,
                data: { CirID: CirID },
                success: function (response) {
                    if (response == 0) {
                        bootbox.alert("Contact to ERP cell");
                        return;

                    }
                    if (response == 1) {

                        //  window.open("../Acedmic/Cyborg_CircularDetail?CirID=" + CirID);
                    }

                }
            });
        }
    }

    function GetCirID(CirID) {
        var split_str = "";
        $.ajax({

            url: '/Media/CheckCirInsert',
            datatype: "json",
            type: 'POST',
            async: false,
            data: { CirID: CirID },
            success: function (response) {
                var dt = response.data;
                var data = JSON.parse(dt);

                if (data.length > 0) {

                    var str = data[0]["CircularId"];
                    split_str = str.split(",");
                }

            }

        });
        return split_str;

    }

    function OnClickMenu(AppTypeID, AppType) {

        $("#AppTypeID").val(AppTypeID);
        $("#AppType").val(AppType);
        var config = "UU";


        var myUrl = '/Account/Cyborg_StudentHome?AppType=' + AppTypeID + '&ApplicationType=' + AppType + '';
        window.location.href = myUrl;

    }

    function Birthday() {

        var SerDate = $.datepicker.parseDate("dd/mm/yy", '16/11/2024');
        var Serday = SerDate.getDate();
        var Sermon = SerDate.getMonth();
        var DOB = $.datepicker.parseDate("dd/mm/yy", '09/10/2005');
        var dobday = DOB.getDate();
        var dobmon = DOB.getMonth();
        if (Sermon == dobmon && Serday == dobday) {
            $("#centralModalSm").modal('show');
        }

    }
</script>

<script type="text/javascript">



    function Showstudentmailpas() {

        $.ajaxSettings.traditional = true;
        $.ajax({
            url: '/Account/Getstudentmailpass',
            datatype: "json",
            type: 'POST',
            data: {},
            success: function (result) {
                var data = JSON.parse(result.State);
                if (data.length > 0) {
                    $("#studentmailid").show();
                    $("#txtstudentmail").text(data[0]["Mail"]);
                    $("#txtstudentpass").text(data[0]["stupassword"]);
                }
                else {

                }
            }
        });

    };

    $(document).ready(function(){


        var Config = 'UU';

        var coll = document.getElementsByClassName("collapsible");
        var i;

        for (i = 0; i < coll.length; i++) {
            coll[i].addEventListener("click", function () {
                this.classList.toggle("active");
                var content = this.nextElementSibling;
                if (content.style.maxHeight) {
                    content.style.maxHeight = null;
                }
                else {
                    content.style.maxHeight = 200 + "px";
                }

                $("#btnclosepop").click(function () {
                    content.style.maxHeight = null;
                });

                window.addEventListener('mouseup', function (event) {
                    if (event.target != content && event.target.parentNode != content) {
                        content.style.maxHeight = null;
                    }
                });
            });
        }

        if(Config == "UU")
        {
            Showstudentmailpas();
            $("#modelFeeReminder").modal("hide");
        }

        if(Config == "SGRRU" || Config == "BFIT")
        {
            $("#modelSGRRUUploadProfilePhoto").modal('show');
        }

        FillWorkshop();

        $("#ddlYesNoCondition").change(function(e){

            $("#ddlWorkShop").val(0);

            if ($("#ddlYesNoCondition").val() == "1"){
                $("#dvWorkShopYes").show();
                $("#dvWorkShopNo").hide();
            }
            else if( $("#ddlYesNoCondition").val() == "2"){
                $("#dvWorkShopYes").hide();
                $("#dvWorkShopNo").show();
            }
            else
            {
                $("#dvWorkShopYes").hide();
                $("#dvWorkShopNo").hide();
            }
        });

        $("#btnSaveWorkShop").click(function(){

            $.ajaxSettings.traditional = true;

            if($("#ddlYesNoCondition").val() == "0"){
                ValidationMsg("Please Select Physically Reporting ? ");
                return;
            }
            else
            {
                if ($("#ddlYesNoCondition").val() == "1"){
                    if ($("#ddlWorkShop").val() == null ||  $("#ddlWorkShop").val() == undefined || $("#ddlWorkShop").val() == "0" || $("#ddlWorkShop").val() == ""){
                        ValidationMsg("Please Select Workshop for Induction Program");
                        return;
                    }
                }
            }

            var params = [];

            params[0] = $("#ddlWorkShop").val() == null || $("#ddlWorkShop").val() == undefined || $("#ddlWorkShop").val() == "" ? "0" : $("#ddlWorkShop").val();
            params[1] = $("#ddlYesNoCondition").val();

            bootbox.confirm(AskforOperation("Are You Sure You Want To " + ($("#ddlYesNoCondition").val() == "1" ? "Take this WorkShop ?" :
                "Denied for this WorkShop ?")), function(e) {

                    if (e == true) {

                        $.ajax({
                            type:"POST",
                            datatype:"json",
                            url:'/Account/InsertWorkSHopData',
                            data:
                            {
                                param:params
                            },
                            success:function(result){
                                if (result == 1){
                                    $("#modelGEUWorkshop").modal('hide');
                                    SuccessMsg("WorkShop Operation Saved Successfully.");
                                    return;
                                }
                                else if(result == 2){
                                    InformationMsg("WorkShop Already Exist.");
                                    return;
                                }
                                else if(result == 3){
                                    InformationMsg("WorkShop Opt limit is full.");
                                    return;
                                }
                                else {
                                    FailedMsg("Something Went wrong please contact to ERP Cell.");
                                    return;
                                }
                            }
                        });
                    }
                });
        });

    });

    function FillWorkshop() {

        $.ajax({
            type: "POST",
            url: '/Account/FillWorkShop',
            data: {
            },
            datatype: "json",
            success: function (response) {
                var dt = response;

                $("#ddlWorkShop").empty();
                var ddlObj = $("#ddlWorkShop");
                var items = ""; var curr = "";
                for (var i = 0; i < dt.length; i++) {
                    items += "<option selected=\"selected\" value='" + dt[i].Value + "'>" + dt[i].Text + "</option>";
                    if (i == 0) {
                        curr = dt[i].Value;
                    }
                }
                ddlObj.html(items).show();
                ddlObj.val(curr);
            }
        });
    }

</script>

<script type="text/javascript">


    function UpdatePhotoInstructions(){
        $('#imgPhoto').attr('src', '../Images/DummyStudent.png').width(160).height(160);
        $("#ModelPhotoInstructions").modal('show');

    }


    function LoadImage(input) {

        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                $('#imgPhoto').attr('src', e.target.result).width(160).height(160);
            };
            reader.readAsDataURL(input.files[0]);
        }
        else {
            $('#imgPhoto').attr('src', '../Images/user.png').width(160).height(160);
        }
    }



    $(document).ready(function(){

        $("#btnSaveImage").click(function (e) {

            e.preventDefault();

            var photodata = new FormData();
            var files = $("#fileImage").get(0).files;

            if (files.length == 0) {
                bootbox.alert("Please choose an image to upload.");
                return;
            }

            var fileName = files[0]['name'];
            var fileExtension = fileName.substr((fileName.lastIndexOf('.')));

            var arrayExtensions = [".jpg", ".jpeg", ".JPG", ".JPEG", ".png", ".PNG", ".bmp", ".BMP"];
            if (arrayExtensions.lastIndexOf(fileExtension) == -1) {
                bootbox.alert("Only JPG, JPEG,PNG file format is supported");
                return;
            }

            if (files[0].size > 122880) {
                bootbox.alert("File size exceeded the 120kb limit");
                return;
            }

            if (files[0].size > 0) {
                photodata.append("helpSectionImages", files[0]);
            } else {
                bootbox.alert("Select a file to upload");
                return;
            }
            var dialog = bootbox.dialog({
                message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
                closeButton: false
            });
            $.ajax({
                datatype: "json",
                type: "POST",
                url: '../SMS/UploadStudentPhoto?RegID=' + 37143 + '',
                processData: false,
                contentType: false,
                data: photodata,
                success: function (response) {
                    if (response == '1') {

                        $("#imgPhoto").val('');
                        LoadImage("$('#fileImage')");
                        $('#ModelPhotoInstructions').modal('hide');
                        location.reload();
                    }
                },
                error: function (er) {

                    return;
                },complete: function(){
                    dialog.modal('hide');
                }
            });
        });
    });
</script>

