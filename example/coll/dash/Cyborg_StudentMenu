<script type="text/javascript">

    $(function() {

        var config = 'UU';

        if (config == 'GEU')
        {
            $("#trStudentIDCard").show();
        }
        else
        {
            $("#trStudentIDCard").hide();
        }

        

    

        $("#PrintTransportCard").click(function() {

            var SYID = "0";
            var RegID = '37143';

            var params = [];

            params[0] = RegID;
            params[1] = SYID;
            params[2] = window.CollegeID;

            var dialog = bootbox.dialog({
                message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
                closeButton: false
            });

            $.ajax({
                url: '/HTM/TransportCardReport',
                datatype: "json",
                type: 'POST',
                data: {
                    param: params
                },
                success: function (result) {

                    dialog.modal('hide');

                    if (result == "1") {
                        window.open('../SMSReport/Cyborg_SMSCommonViewer', '_blank');
                        return;
                    }
                    else if (result == "2") {
                        bootbox.alert("No Record Found.");
                        return;
                    }
                    else {
                        bootbox.alert("Some Thing Went Wrong, Please Contact To ERP Cell.");
                        return;
                    }

                },

            });

        });

    });

    $(document).ready(function () {

        $("#btnSubmit").click(function () {

            bootbox.confirm("Are you sure you want to submit your suggestion", function (result) {




                if (result) {

                    $.ajaxSettings.traditional = true;

                    var Suggestion = $("#txtSuggestion").val().trim();
                    $.ajax({
                        url: '/Account/InsertingSuggestion',
                        datatype: "json",
                        type: 'POST',
                        data: { Suggestion:Suggestion},
                        success: function (result) {


                            if (result == 1) {
                                bootbox.alert("Suggestion saved successfully");
                                $('#modalSuggestion').modal('hide');
                                return;
                            }
                            else {
                                bootbox.alert("Oops Something Went's Wrong. Please try Again!!!")
                                return;
                            }
                        }
                    })
                }
            });


        });
    });

    function ShowpopupSuggestion() {

        $("#txtSuggestion").val('');
        $("#modalSuggestion").modal("show");

    }

    function PrintIdCard()
    {
       

        var dialog = bootbox.dialog({
            message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
            closeButton: false
        });

        $.ajax({
            type: "POST",
            url: '/Account/StudentIDCardPrint',
            data: { RegID: '37143' },
            datatype: "json",
            success: function (response) {
                dialog.modal('hide');

                var dt = JSON.parse(response);

                if (dt.length == 0) {
                    bootbox.alert("No record found");
                    return;
                }
                else {
                    window.open('../SMSReport/Cyborg_SMSCommonViewer', '_blank');
                    return;
                }
            }
        });
      
    }

</script>

<script type="text/javascript">
    $(document).ready(function()
    {
        $("#btnAdd1").click(function()
        {
            $("#Row2").show();
            $("#btnAdd1").hide();
            $("#txtCourseTitle2").val("");
            $("#txtProgramName2").val(0);
            $("#FileCertificate2").val(null);


        });

        $("#btnSub2").click(function()
        {
            $("#Row2").hide();
            $("#btnAdd1").show();
            $("#txtCourseTitle2").val("");
            $("#txtProgramName2").val(0);
            $("#FileCertificate2").val(null);
        });

        $("#btnAdd2").click(function(){
            $("#Row3").show();
            $("#btnAdd2").hide();
            $("#btnSub2").hide();
            $("#txtCourseTitle3").val("");
            $("#txtProgramName3").val(0);
            $("#FileCertificate3").val(null);
        });

