            GEUInstruction = GEUInstruction + "</div>"
            GEUInstruction = GEUInstruction + "<div class='row'>"
            GEUInstruction = GEUInstruction + "<div class='col-sm-1'>c)</div>"
            GEUInstruction = GEUInstruction + "<div class='col-sm-11'>Other than Engineering (Non-Engineering) Students  January 11, 2021</div>"
            GEUInstruction = GEUInstruction + "</div>"
        }
        else ///////// GEHU
        {
            GEUInstruction = GEUInstruction + "<div class='row'>"
            GEUInstruction = GEUInstruction + "<div class='col-sm-1'>a)</div>"
            GEUInstruction = GEUInstruction + "<div class='col-sm-11'>Engineering Students + Design Students (B.Tech, B.Sc Animation & Gaming, BA Visual Arts, Bachelor of Fashion Design, B. Arch) (except First Year)  January 11, 2021</div>"
            GEUInstruction = GEUInstruction + "</div>"
            GEUInstruction = GEUInstruction + "<div class='row'>"
            GEUInstruction = GEUInstruction + "<div class='col-sm-1'>b)</div>"
            GEUInstruction = GEUInstruction + "<div class='col-sm-11'>All Senior Students (Non-First Year) except Engineering & Design Students  January 18, 2021</div>"
            GEUInstruction = GEUInstruction + "</div>"
            GEUInstruction = GEUInstruction + "<div class='row'>"
            GEUInstruction = GEUInstruction + "<div class='col-sm-1'>c)</div>"
            GEUInstruction = GEUInstruction + "<div class='col-sm-11'>All First Year Students  January 20, 2021</div>"
            GEUInstruction = GEUInstruction + "</div>"
        }

        GEUInstruction = GEUInstruction + "<b>Kindly note that</b>"
