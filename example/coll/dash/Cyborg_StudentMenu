<script type="text/javascript">

    function FillSubstituteData(dt)
    {

        $.jgrid.gridUnload('jqgrdSelectAllSubstitute');
        $("#jqgrdSelectAllSubstitute").jqGrid({
            datatype: "jsonstring",
            mtype: 'GET',
            autosize: true,
            height: '300',
            width: null,
            shrinkToFit: false,
            colNames: ['','Applicable From', 'Time', 'Period', 'Subs. Date', 'Subject', 'Employee', 'Substitute Subject', 'Substitute Employee'],
            colModel: [
                        { name: 'SortID', index: 'SortID', width: 80, align: 'left', stype: "select", searchoptions: { value: "Active:Active;Old:Old" } },
                        { name: 'ApplyDate', index: 'ApplyDate', width: 130, align: 'left' },
                        { name: 'Duration', index: 'Duration', width: 100, align: 'left' },
                        { name: 'Period', index: 'Period', width: 80, align: 'center' },
                        { name: 'SubsDate', index: 'SubsDate', width: 90, align: 'left' },

                        {
                            name: 'Subject', index: 'Subject', width: 200, align: 'left', cellattr: function (rowId, tv, rawObject, cm, rdata) {
                                return 'style="white-space: normal;"'
                            }
                        },
                        {
                            name: 'Employee', index: 'Employee', width: 160, align: 'left', cellattr: function (rowId, tv, rawObject, cm, rdata) {
                                return 'style="white-space: normal;"'
                            }
                        },
                        {
                            name: 'SubsSubject', index: 'SubsSubject', width: 200, align: 'left', cellattr: function (rowId, tv, rawObject, cm, rdata) {
                                return 'style="white-space: normal;"'
                            }
                        },
                        {
                            name: 'SubsEmployee', index: 'SubsEmployee', width: 160, align: 'left', cellattr: function (rowId, tv, rawObject, cm, rdata) {
                                return 'style="white-space: normal;"'
                            }
                        },
            ],

            datastr: dt,
            rownumbers: true,
            rowNum: 10000,
            sortable: false,
            viewrecords: true,
            loadonce: true,
            caption: "Active Substitution",
            multiselect: false,
            paging: true,
            align: "center",
            pager: "#pgrSelectAllSubstitute"
        });
        $("#jqgrdSelectAllSubstitute").jqGrid('filterToolbar', { stringResult: true, searchOnEnter: false, defaultSearch: "cn" });

        $("#gs_SortID").trigger("change");
    }

    function fillTimeTable(dtClassRegister, col, mod)
    {



        $.jgrid.gridUnload('jqgrdTimeTable');

        var ct = col;
        var cm = mod;
        var dte = "";


        var uFields = ct.split(',');
        var columns = [];

        for (var i = 0; i < uFields.length ; i++) {
            columns.push(uFields[i]);
        }

        var uFields2 = cm.split(',');
        var modals = [];
        for (var i = 0; i < uFields2.length ; i++) {
            if (uFields2[i] == "Day/Period") {
                modals.push({
                    name: uFields2[i], index: uFields2[i], width: 100, align: 'left', editable: true, edittype: 'select',
                    cellattr: function (rowId, tv, rawObject, cm, rdata) { return 'style="white-space: normal;"' }
                });

            }



            else {
                modals.push({ name: uFields2[i], index: uFields2[i], width: 150, search: false, align: 'left', editable: false, sortable: false, cellattr: function (rowId, tv, rawObject, cm, rdata) { return 'style="white-space: normal;"' } });
            }
        }

        $("#jqgrdTimeTable").jqGrid({
            datatype: "jsonstring",
            mtype: 'GET',
            autosize: true,
            height: "auto",
            width: null,
            shrinkToFit: false,
            colNames: columns,
            colModel: modals,
            caption:"Today's Time Table",
            datastr: dtClassRegister,
            rownumbers: true,
            rowNum: 70,
            viewrecords: true,
            loadonce: true,
            paging: true,
            // multiselect: true,
            align: "center",
            //pager: "#pgrTimeTable"

        });

        //$("#jqgrdTimeTable").jqGrid('filterToolbar', { stringResult: true, searchOnEnter: false, defaultSearch: "cn" });
        //$("#jqgrdTimeTable").jqGrid('setFrozenColumns');
    }



    function GetAllSubstitute(chk) {



        $.ajaxSettings.traditional = true;

        var dialog = bootbox.dialog({
            message: '<p class="text-center mb-0"><i class="fa fa-spin fa-spinner"></i>  Please wait ...</p>',
            closeButton: false
        });

        $.ajax({
            url: '/Account/GetAllSubstitute',
            datatype: "json",
            type: 'POST',
            data: {
                chk: chk
            },
            success: function (response) {
                dialog.modal('hide');
                $("#modelAllSubstitute").modal('show');

                var dt = response.state;

                FillSubstituteData(dt);
                var dtClassRegister = response.timetable;
                var col = response.col;
                var mod = response.mod;
                fillTimeTable(dtClassRegister, col, mod);

                //$("#jqgrdSelectEmployee").jqGrid('setFrozenColumns');
            }
        });
    }

    function validateEmail(email) {

        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }

    function isNumber(evt) {
        evt = (evt) ? evt : window.event;
        var charCode = (evt.which) ? evt.which : evt.keyCode;
        if (charCode == 8 || charCode == 46) {
            return true;
        }
        if (charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }
        if (charCode < 48 || charCode > 57) {
            return false;
        }
        return true;
    }

    $(document).ready(function () {



        var config = 'UU';
        if (config == 'UU' || config == 'QGC')
        {
            $("#linkUpdateContact").show();
        }
        else
        {
            $("#linkUpdateContact").hide();
        }


        $("#rdbActive").change(function () {
            if ($("#rdbActive").prop("checked", true)) {
                $('#rdbNonActive').prop('checked', false);

                //FillMemberDetails(1);
                //FillPropAssignMemberList(1);
            }

        });


        $("#rdbNonActive").change(function () {
            if ($("#rdbNonActive").prop("checked", true)) {
                $('#rdbActive').prop('checked', false);
                //FillMemberDetails(2)
                //FillPropAssignMemberList(2);
            }

        });

        $("#btnSubstituteMore").click(function (e) {
            GetAllSubstitute(1);
        });

        $("#linkUpdateContact").click(function (e) {

            if('UU' == 'UU'){
                $("#dvMobile_Update").hide();
                $("#dvMobile_UpdateBreak").hide();
                $("#dvAlternateMobile_Update").hide();
                $("#dvAlternateMobile_UpdateBreak").hide();
            }
            else
            {
                $("#dvMobile_Update").show();
                $("#dvMobile_UpdateBreak").show();
                $("#dvAlternateMobile_Update").show();
                $("#dvAlternateMobile_UpdateBreak").show();
            }

            $("#modelUpdateContact").modal('show');
        });

        $("#btnUpdateContact").click(function (e) {
            e.preventDefault();

            if ($("#txtMobile").val().trim() == "") {
                bootbox.alert("Please Enter Mobile No.");
                return;
            }
            if ($("#txtMobile").val().trim().length < 10) {
                bootbox.alert("Please Enter valid Mobile No.");
                return;
            }
            if ($("#txtAlternateMobile").val().trim() != "") {
                if ($("#txtAlternateMobile").val().trim().length < 10) {
                    bootbox.alert("Please Enter valid Mobile No.");
                    return;
                }
            }

            if ($("#txtEmailID").val().trim() == "") {
                bootbox.alert("Please Enter EmailID");
                return;
            }
            if (validateEmail($("#txtEmailID").val().trim()))
            {

            }
            else
            {
                bootbox.alert("Please Enter Valid EmailID");
                return;
            }

            var MobileNo = $("#txtMobile").val().trim();
            var AlternateMobileNo = $("#txtAlternateMobile").val().trim();
            var EmailID = $("#txtEmailID").val().trim();
            var NADID = $("#txtNADID").val().trim();
            bootbox.confirm("Are you sure you want to update ?", function (result) {
                if (result) {
                    $.ajax({
                        url: '/Web_StudentAcademic/UpdateStudentContactDetails',
                        datatype: "json",
                        type: 'POST',
                        data: { MobileNo: MobileNo, AlternateMobile: AlternateMobileNo, EmailID: EmailID, NADID:NADID },
                        success: function (result) {
                            if (result == "0") {
                                bootbox.alert("Please Contact to Erp Cell");
                                return;
                            }
                            if (result == "1") {
                                bootbox.alert("Contact Information Updated Successfully");
                                return;
                            }
                        },
                        error: function () {
                        }
                    });
                }
            });
        });

    });

</script>

<script type="text/javascript">

    function GetBusRoute() {
        $.ajax({
            type: "POST",
            url: '/Account/GetStudentWiseRoute',
            data: {
            },
            datatype: "json",
            success: function (response) {
                var dt = (response);

                $("#ddlBusRoute").empty();
                var ddlObj = $("#ddlBusRoute");

                var items = ""; var curr = "";
                for (var i = 0; i < dt.length; i++) {
                    items += "<option selected=\"selected\" value='" + dt[i].Value + "'>" + dt[i].Text + "</option>";
                }
                ddlObj.html(items).show();
                ddlObj.val(0);
            }
        });
    }

    function GetBusRouteInstruction() {
        $.ajax({
            type: "POST",
            url: '/Account/GetStudentWiseRoute',
            data: {
            },
            datatype: "json",
            success: function (response) {

                var dt = (response);

                $("#ddlBusRouteInstruction").empty();
                var ddlObj = $("#ddlBusRouteInstruction");

                var items = ""; var curr = "";
                for (var i = 0; i < dt.length; i++) {
                    items += "<option selected=\"selected\" value='" + dt[i].Value + "'>" + dt[i].Text + "</option>";
                }
                ddlObj.html(items).show();
                ddlObj.val(0);

            }
        });
    }

    $(document).ready(function () {

        //  $("#btnSubmitClassMode").hide();
        var Config = 'UU';
        if (Config == "UU") {
            $("#dvHelplineNo").show();
            $("#footerContectNo").empty();
            $("#footerContectNo").text("");   //7251096625   
            $("#dvUULibrary").show();

        }
        else {
            $("#footerText").css('display','none')
             $("#dvHelplineNo").hide();
            $("#dvUULibrary").hide();
        }

        


        $("#ddlClassMode").change(function () {
            if ($("#ddlClassMode").val() != '0') {
                $("#dvCommand").show();
                $("#dvConfirmText").show();
            } else {
                $("#dvCommand").hide();
                $("#dvConfirmText").hide();
            }

            //if ($("#ddlClassMode").val() == '1') {
            //    $("#dvOffine").hide();
            //}
            //if ($("#ddlClassMode").val() == '2') {
            //    $("#dvOffine").show();
            //}
        });

        $("#btnReview").click(function (e) {
            e.preventDefault();
            $("#ddlClassMode").val('0');
            $("#dvCommand").hide();
            $("#dvConfirmText").hide();
        });

        $("#btnConfirm").click(function (e) {
            e.preventDefault();
            if ($("#ddlClassMode").val() == '2') {
                $("#dvOffine").show();
                $("#btnSubmitClassMode").show();
            } else if ($("#ddlClassMode").val() == '1') {
                $("#btnSubmitClassMode").hide();
                $.ajaxSettings.traditional = true;
                var param = [];

                param[0] = $("#ddlClassMode").val();
                param[1] = $("#ddlConfirmHostel").val();
                param[2] = $("#ddlConfirmTransport").val();
                param[3] = $("#ddlBusRoute").val();

                var dialog = bootbox.dialog({
                    message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
                    closeButton: false
                });

                $.ajax({
                    type: "POST",
                    url: '/Account/InsertStuClassChoice',
                    data: { param: param },
                    datatype: "json",
                    success: function (response) {
                        dialog.modal('hide');
                        if (response == 1) {
                            bootbox.confirm('<b>Thank you for your confirmation! Your classes will continue in the online mode for the entire semester. Please make sure youre attending all of them! Stay Safe, Stay Home! </b>', function (result) {
                                //$('#dvGEUOnlineOffine').modal('hide');

                                window.location.href = '../Account/Cyborg_StudentMenu';
                            });
                        } else {
                            bootbox.alert("Problem in process. Please contact to ERP-Cell");
                        }
                    }
                });
            }

            $("#dvMode").hide();
            $("#dvConfirmText").hide();
            $("#dvCommand").hide();
        });

        $("#ddlConfirmOffline").change(function () {
            if ($("#ddlConfirmOffline").val() == '1') {
                if(30 != 4){
                    $("#dvOffineConfirm").show();
                    $("#dvIsHostel").show();
                }
            } else {
                $("#dvOffineConfirm").hide();
                $("#dvIsHostel").hide();
                $("#dvBusRoute").hide();
                $("#ddlBusRoute").val('0');
                $("#dvOffine").hide();
                $("#dvMode").show();
                $("#dvConfirmText").show();
                $("#dvCommand").show();
                $("#ddlConfirmOffline").val('0');
                $("#btnSubmitClassMode").hide();
                $("#dvIsTransport").hide();

                $("#ddlConfirmHostel").val('0');
                $("#ddlConfirmTransport").val('0');
                $("#ddlBusRoute").val('0');

            }
        });

        $("#ddlConfirmHostel").change(function () {
            if ($("#ddlConfirmHostel").val() == '2') {
                $("#dvIsTransport").show();
            } else {
                $("#dvIsTransport").hide();
                $("#dvBusRoute").hide();
                $("#ddlBusRoute").val('0');
                $("#ddlConfirmTransport").val('0');
                $("#ddlBusRoute").val('0');
            }
        });


        $("#ddlConfirmTransport").change(function () {
            if ($("#ddlConfirmTransport").val() == '1') {
                if(30 != '3' && 30 != '4')
                {
                    $("#dvBusRoute").show();
                    $("#ddlBusRoute").show();
                }
            } else {
                $("#dvBusRoute").hide();
                $("#ddlBusRoute").hide();
                $("#ddlBusRoute").val('0');
            }
        });

        $("#btnSubmitClassMode").click(function (e) {
            e.preventDefault();

            if ($("#ddlIsVaccinated").val() == null || $("#ddlIsVaccinated").val() == '0') {
                $('#ddlIsVaccinated').attr('data-original-title', 'Please select.')
                    .tooltip('fixTitle')
                    .tooltip('show');
                $("#ddlIsVaccinated").focus();
                return;
            }

            if ($("#ddlClassMode").val() == null || $("#ddlClassMode").val() == '0') {
                $('#ddlClassMode').attr('data-original-title', 'Please select a class mode')
                    .tooltip('fixTitle')
                    .tooltip('show');
                $("#ddlClassMode").focus();
                return;
            }




            

            $.ajaxSettings.traditional = true;
            var param = [];

            param[0] = $("#ddlClassMode").val();
            param[1] = $("#ddlIsVaccinated").val();//$("#ddlConfirmHostel").val();
            param[2] = 0;//$("#ddlConfirmTransport").val();
            param[3] = 0;//$("#ddlBusRoute").val();


            var dialog = bootbox.dialog({
                message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
                closeButton: false
            });

            $.ajax({
                type: "POST",
                url: '/Account/InsertStuClassChoice',
                data: { param: param },
                datatype: "json",
                success: function (response) {
                    dialog.modal('hide');
                    if (response == 1) {
                        bootbox.confirm('<b>Your preferences has been successfully submitted ?</b>', function (result) {
                            // $('#dvGEUOnlineOffine').modal('hide');
                            window.location.href = '../Account/Cyborg_StudentMenu';
                        });
                    } else {
                        bootbox.alert("Problem in process. Please contact to ERP-Cell");
                    }
                }
            });

        });



        $("#btnSubmitEmailMobileNo").click(function (e) {
            var Email = $("#txtStuEmail").val();
            var PhoneNo = $("#txtStuMobile").val();
            var checkboxValue = document.getElementById("chkFinalUpdate").checked ? 1 : 0;

            var checkBox = 

            bootbox.confirm("<b>Are you sure you want to Update Details?</b>", function (result) {
                if (result) {
                    var dialog = bootbox.dialog({
                        message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
                        closeButton: false
                    });
                    $.ajax({
                        type: "POST",
                        url: '../Account/UpdateStudentEmailAndMobileNo',
                        data: {
                            Email: Email,
                            PhoneNo: PhoneNo,
                            checkboxValue: checkboxValue
                        },
                        datatype: "json",
                        success: function (response) {
                            dialog.modal('hide');
                            if (response == '-1') {
                                bootbox.alert('<b>Details has been updated successfully.</b>');
                                $("#modalVerificatioMobileNoAndEmail").modal('hide');
                                return;
                            } else {
                                bootbox.alert('<b>Something went wrong, Please Contact to ERP.</b>');
                                return;
                            }
                        }
                    });
                }
            });
        });


    });
</script>

<script type="text/javascript">

    $(document).ready(function(){

        $("#txtTravelDate").datepicker({
            dateFormat: "dd/mm/yy",
            minDate: '16/11/2024',
            //maxDate: '+0D',
            //minDate: minDate,
            changeMonth: true,
            changeYear: true,
            yearRange: "-1:+0",
        });
        $("#txtArrivalDate").datepicker({
            dateFormat: "dd/mm/yy",
            minDate: '16/11/2024',
            //maxDate: '+0D',
            //minDate: minDate,
            changeMonth: true,
            changeYear: true,
            yearRange: "-1:+0",
        });

        $("#btnSubmit").click(function(e) {
            e.preventDefault();

            if ($("#ddlTravelMode").val() == null || $("#ddlTravelMode").val() == '0') {
                $('#ddlTravelMode').attr('data-original-title', 'Select travel mode')
                 .tooltip('fixTitle')
                 .tooltip('show');
                $("#ddlTravelMode").focus();
                return;
            }
            if($("#ddlTravelMode").val() == '3') {
                if ($("#ddlVehicle").val() == null || $("#ddlVehicle").val() == '0') {
                    $('#ddlVehicle').attr('data-original-title', 'Select vehicle mode')
                     .tooltip('fixTitle')
                     .tooltip('show');
                    $("#ddlVehicle").focus();
                    return;
                }
            }

            if ($("#txtPlaceFrom").val() == null || $("#txtPlaceFrom").val() == '') {
                $('#txtPlaceFrom').attr('data-original-title', 'Enter travel source place')
                 .tooltip('fixTitle')
                 .tooltip('show');
                $("#txtPlaceFrom").focus();
                return;
            }

            if ($("#txtTravelDate").val() == null || $("#txtTravelDate").val() == '') {
                $('#txtTravelDate').attr('data-original-title', 'Enter Travel Date')
                 .tooltip('fixTitle')
                 .tooltip('show');
                $("#txtTravelDate").focus();
                return;
            }

            var TF = $("#txtTimeFrom").val().trim();
            var TT = $("#txtTimeTo").val().trim();

            if (TF == "") {
                $('#txtTimeFrom').attr('data-original-title', 'Please Pick Valid Travel Time.')
                .tooltip('fixTitle')
                .tooltip('show');
                $("#txtTimeFrom").focus();
                return;
            }

            if ($("#txtArrivalDate").val() == null || $("#txtArrivalDate").val() == '') {
                $('#txtArrivalDate').attr('data-original-title', 'Enter Arrival Date')
                 .tooltip('fixTitle')
                 .tooltip('show');
                $("#txtArrivalDate").focus();
                return;
            }

            if (TT == "") {
                $('#txtTimeTo').attr('data-original-title', 'Please Pick Valid Arrival Time.')
                .tooltip('fixTitle')
                .tooltip('show');
                $("#txtTimeTo").focus();
                return;
            }


            if ($("#txtTotalPerson").val() == null || $("#txtTotalPerson").val() == ''
                || $("#txtTotalPerson").val() == '0') {
                $('#txtTotalPerson').attr('data-original-title', 'Enter Total Person')
                 .tooltip('fixTitle')
                 .tooltip('show');
                $("#txtTotalPerson").focus();
                return;
            }

            var params = [];

            params[0] = $("#ddlTravelMode").val();
            params[1] = $("#ddlVehicle").val();
            params[2] = $("#txtPlaceFrom").val().trim();
            params[3] = $("#txtTimeFrom").val();
            params[4] = $("#txtTimeTo").val();
            params[5] = $("#txtTravelDate").val();
            params[6] = $("#txtArrivalDate").val();
            params[7] = $("#txtTotalPerson").val();

            bootbox.confirm("<b>Are You Sure You Want To Save?</b>", function (result) {
                if (result) {

                    var dialog = bootbox.dialog({
                        message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
                        closeButton: false
                    });

                    var photodata = new FormData();
                    $.ajax({
                        url: '../Account/Insert_CovidTravelDetail?parameters=' + JSON.stringify(params) + '',
                        datatype: "json",
                        type: 'POST',
                        processData: false,
                        contentType: false,
                        data: photodata,
                        success: function (result) {

                            dialog.modal('hide');

                            if (result == "1") {
                                $('#dvTravelDetail').modal('hide');
                                bootbox.alert('<img src="../img/submit.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp;Record Submitted Successfully.</b>');
                                return;
                            }
                            else {
                                bootbox.alert('<img src="../img/failure.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp;Please Contact To ERP Cell.</b>');
                                return;
                            }
                        }
                    });

                }
            });
        });

        $("#ddlTravelMode").change(function(){
            if($("#ddlTravelMode").val() == '3') {
                $("#dvVehicle").show();
                $("#dvVehicleType").show();
            }else{
                $("#dvVehicle").hide();
                $("#dvVehicleType").hide();
                $("#dvVehicleType").val('0');
            }
        });
    });
</script>

<script type="text/javascript">
    $(document).ready(function()
    {
        $("#btnSaveProgram").click(function()
        {
            if($("#ddlExchangeProg").val()==null || $("#ddlExchangeProg").val()==undefined || $("#ddlExchangeProg").val()=="" || $("#ddlExchangeProg").val()==0)
            {
                bootbox.alert("Please select Program Type.");
                return;
            }

            var RegID=$("#hdnStuRegisID").val();
            var ProgramID=$("#ddlExchangeProg").val();

            if (RegID==0)
            {
                bootbox.alert("Please Refresh your Page or Login again.");
                return;
            }

            $.ajaxSettings.traditional = true;

            $.ajax({
                type: "POST",
                url: '/SMS/InsertExchangeProgram',
                data:
                    {
                        RegID: RegID,ProgramID:ProgramID
                    },
                datatype: "json",
                success: function (result)
                {
                    if (result == 1)
                    {
                        $('#modelExchangeProgram').modal('hide');
                        bootbox.alert(" Program Applied Successfully.");
                        return;
                    }
                    else
                    {
                        bootbox.alert("SomeThing went wrong. Please contact to ERP-Cell");
                    }
                }
            });

        });

    });
</script>

<script type="text/javascript">

    var Consent = "";

    function FillInstruction(UniversityID,University,Course,EnrollmentNo,StudentName,FatherName){

        var GEUInstruction = "";
        var CommonPart = "";

        /////////////////////////   Instruction Part Start

        GEUInstruction = GEUInstruction + "<b>Dear Student</b>"
        GEUInstruction = GEUInstruction + "<br/><br/>"
        GEUInstruction = GEUInstruction + "<p style=\"text-align:justify;\">We are pleased to inform that State Government of Uttarakhand has permitted teaching-learning in the offline mode with effect from 15/12/2020. However, it has issued detailed guidelines. Kindly study these guidelines along with your parents with utmost sincerity since you will be bound to follow these stringently.</p>"
        GEUInstruction = GEUInstruction + "<br/>"
        GEUInstruction = GEUInstruction + "It is important to reproduce following important guidelines:"
        GEUInstruction = GEUInstruction + "<br/>"
        GEUInstruction = GEUInstruction + "<div class='row'>"
        GEUInstruction = GEUInstruction + "<div class='col-sm-1'></div>"
        GEUInstruction = GEUInstruction + "<div class='col-sm-11'>Consent of the parents allowing their wards for offline teaching-learning is mandatory. The Consent form is being provided further down this form, and your acceptance of the same is mandatory in order for you to be allowed to attend the Offline Mode of Teaching. The consent submitted on or before the 28th of December, 2020, will be considered. Any submissions later than this will be considered invalid and the concerned student will not be permitted for the Offline mode of teaching.</div>"
        GEUInstruction = GEUInstruction + "</div>"
        GEUInstruction = GEUInstruction + "<div class='row'>"
        GEUInstruction = GEUInstruction + "<div class='col-sm-1'></div>"
        GEUInstruction = GEUInstruction + "<div class='col-sm-11'>Offline teaching-learning can be held only where practicals are a mandatory requirement. </div>"
        GEUInstruction = GEUInstruction + "</div>"
        GEUInstruction = GEUInstruction + "<div class='row'>"
        GEUInstruction = GEUInstruction + "<div class='col-sm-1'></div>"
        GEUInstruction = GEUInstruction + "<div class='col-sm-11'>Doubt clearing interactions will be allowed only with prior appointment with the faculty concerned. </div>"
        GEUInstruction = GEUInstruction + "</div>"

