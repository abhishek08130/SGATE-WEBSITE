        /////////////////////////   Consent Part End

        $("#dvGEUInstruction").html(GEUInstruction);
    }

    function hideshowOnlineOfflineRadioButton(){

        $("#rdbtnOnline").prop('checked', false);
        $("#rdbtnOffline").prop('checked', false);
        $("#chkIhaveReadConsentForm").prop('checked', false);
        $("#dvIhaveReadConsentForm").hide();
        $("#dvGEUConsent").html("");

        if ($("#chkIhaveReadInstruction").prop('checked') == true){
            $("#dvRadioOnline").show();
            $("#dvRadioOffline").show();
        }
        else{
            $("#dvRadioOnline").hide();
            $("#dvRadioOffline").hide();
            $("#btnSubmitClassModeInstruction").hide();
        }
    }

    function hideshow(){

        $("#chkIhaveReadConsentForm").prop('checked', false);

        if ($("#rdbtnOnline").prop("checked") == true){
            $("#btnSubmitClassModeInstruction").show();
            $("#dvGEUConsent").html("");
            $("#dvIhaveReadConsentForm").hide();
        }
        else{
            $("#btnSubmitClassModeInstruction").hide();
            $("#dvGEUConsent").html(Consent);
            $("#dvIhaveReadConsentForm").show();
        }
    }

    function RedirectToBookRoom(){
        window.location.href = "../HTM/Cyborg_HTM_Student_ViewHostelGallery";
    }

    $(function () {

        $("#btnSubmitClassModeInstruction").hide();
        $("#btnSubmitHostelTransport").hide();
        $("#btnRedirectToPay").hide();
        $("#dvIhaveReadConsentForm").hide();

        $("#ddlConfirmTransportInstruction").val('0');
        $("#ddlConfirmHostelInstruction").val('0');
        $("#ddlBusRouteInstruction").val('0');

        $("#dvHostelInstruction").hide();
        $("#dvTransportInstruction").show();

        $("#chkIhaveReadConsentForm").change(function () {
            if ($("#chkIhaveReadConsentForm").prop("checked") == true){
                $("#btnSubmitClassModeInstruction").show();
            }
            else
            {
                $("#btnSubmitClassModeInstruction").hide();
            }
        });

        $("#ddlConfirmHostelInstruction").change(function () {

            if ($("#ddlConfirmHostelInstruction").val() == '2') {

                $("#btnSubmitHostelTransport").hide();
                $("#btnRedirectToPay").hide();
                $("#dvHostelYesInstruction").hide();

                if (30 != '3')
                {
                    $("#dvTransportInstruction").show();
                }
                else
                {
                    $("#btnSubmitHostelTransport").show();
                }
            }
            else if ($("#ddlConfirmHostelInstruction").val() == '0') {
                $("#btnSubmitHostelTransport").hide();
                $("#btnRedirectToPay").hide();
                $("#dvTransportInstruction").hide();
                $("#dvHostelYesInstruction").hide();
            }
            else {
                $("#dvTransportInstruction").hide();
                $("#dvBusRouteInstruction").hide();
                $("#ddlBusRouteInstruction").val('0');
                $("#ddlConfirmTransportInstruction").val('0');
                $("#btnSubmitHostelTransport").hide();
                $("#btnRedirectToPay").show();
                $("#dvHostelYesInstruction").show();
            }

        });

        $("#ddlConfirmTransportInstruction").change(function () {

            $("#btnRedirectToPay").hide();

            if ($("#ddlConfirmTransportInstruction").val() == '1') {

                $("#btnSubmitHostelTransport").hide();

                if(30 != '1' || 30 != '2')
                {
                    $("#dvBusRouteInstruction").show();
                    $("#ddlBusRouteInstruction").show();
                }
                else
                {
                    $("#btnSubmitHostelTransport").show();
                }
            }
            else if ($("#ddlConfirmTransportInstruction").val() == '0') {
                $("#btnSubmitHostelTransport").hide();
                $("#dvBusRouteInstruction").hide();
                $("#ddlBusRouteInstruction").hide();
            }
            else {
                $("#dvBusRouteInstruction").hide();
                $("#ddlBusRouteInstruction").hide();
                $("#ddlBusRouteInstruction").val('0');

                $("#btnSubmitHostelTransport").show();
            }
        });

        $("#ddlBusRouteInstruction").change(function () {

            $("#btnRedirectToPay").hide();

            if ($("#ddlBusRouteInstruction").val() == '0') {
                $("#btnSubmitHostelTransport").hide();
            }
            else
            {
                $("#btnSubmitHostelTransport").show();
            }
        });


        $("#btnSubmitClassModeInstruction").click(function (e) {
            e.preventDefault();
            $.ajaxSettings.traditional = true;

            var param = [];
            param[0] = $("#rdbtnOnline").prop("checked") == true ? 1 : 2;
            param[1] = 0;
            param[2] = 0;
            param[3] = 0;

            bootbox.confirm({
                message: "<b>Are you sure you wish to continue with the selected option. Please note that your choice here will be considered final and will be treated as irreversible.</b>",
                buttons: {
                    cancel: {
                        label: '<i class="fa fa-times"></i> NO',
                        className: 'btn-danger'
                    },
                    confirm: {
                        label: '<i class="fa fa-check"></i> YES',
                        className: 'btn-success'
                    }
                },
                callback: function (result) {

                    if (result){

                        var dialog = bootbox.dialog({
                            message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
                            closeButton: false
                        });

                        $.ajax({
                            type: "POST",
                            url: '/Account/InsertStuClassChoice',
                            data: { param: param },
                            datatype: "json",
                            success: function (response) {
                                dialog.modal('hide');
                                if (response == 1) {
                                    bootbox.confirm('<b>Your Submission Complete. </b>', function (result) {
                                        window.location.href = '../Account/Cyborg_StudentMenu';
                                    });
                                } else {
                                    bootbox.alert("Problem in process. Please contact to ERP-Cell");
                                }
                            }
                        });
                    }
                }

            });
        });

        $("#btnSubmitHostelTransport").click(function (e) {
            e.preventDefault();

            //if ($("#ddlConfirmHostelInstruction").val() == null || $("#ddlConfirmHostelInstruction").val() == '0') {
            //    $('#ddlConfirmHostelInstruction').attr('data-original-title', 'Please Select Hostel Option')
            //        .tooltip('fixTitle')
            //        .tooltip('show');
            //    $("#ddlConfirmHostelInstruction").focus();
            //    return;
            //}

            //if ($("#ddlConfirmHostelInstruction").val() == '2') {
            if (($("#ddlConfirmTransportInstruction").val() == null || $("#ddlConfirmTransportInstruction").val() == '0')
                && '30' != '1' && '30' != '2') {
                $('#ddlConfirmTransportInstruction').attr('data-original-title', 'Please Select Transport Option')
                    .tooltip('fixTitle')
                    .tooltip('show');
                $("#ddlConfirmTransportInstruction").focus();
                return;
            }

            if ($("#ddlConfirmTransportInstruction").val() == '1') {
                if (($("#ddlBusRouteInstruction").val() == null || $("#ddlBusRouteInstruction").val() == '0')
                    && '30' != '1' && '30' != '2') {
                    $('#ddlBusRouteInstruction').attr('data-original-title', 'Please Select A Bus Route.')
                            .tooltip('fixTitle')
                            .tooltip('show');
                    $("#ddlBusRouteInstruction").focus();
                    return;
                }
            }
            //}

            $.ajaxSettings.traditional = true;
            var param = [];
            param[0] = $("#ddlConfirmHostelInstruction").val();
            param[1] = $("#ddlConfirmTransportInstruction").val();
            param[2] = $("#ddlBusRouteInstruction").val();

            bootbox.confirm({
                message: "<b>Are you sure you wish to continue with the selected option .</b>",
                buttons: {
                    cancel: {
                        label: '<i class="fa fa-times"></i> NO',
                        className: 'btn-danger'
                    },
                    confirm: {
                        label: '<i class="fa fa-check"></i> YES',
                        className: 'btn-success'
                    }
                },
                callback: function (result) {

                    if (result){

                        var dialog = bootbox.dialog({
                            message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
                            closeButton: false
                        });

                        $.ajax({
                            type: "POST",
                            url: '/Account/UpdateHostelTransportChoice',
                            data: { param: param },
                            datatype: "json",
                            success: function (response) {
                                dialog.modal('hide');
                                if (response == 1) {
                                    bootbox.confirm('<b>Your Transport Submission Complete. </b>', function (result) {
                                        window.location.href = '../Account/Cyborg_StudentMenu';
                                    });
                                } else {
                                    bootbox.alert("Problem in process. Please contact to ERP-Cell");
                                }
                            }
                        });
                    }
                }

            });
        });

    });

</script>

<script type="text/javascript">

    function FeeBalanceAmountWiseSemesterRegistration() {
        $.ajax({
            type: "POST",
            url: '/Account/FeeBalanceAmountWiseSemesterRegistration',
            data: {
            },
            datatype: "json",
            success: function (response) {
                var dt = JSON.parse(response);
                if(dt.length > 0){
                    if(parseInt(dt[0]["toCheck"]) == 1 && parseInt(dt[0]["BalanceAmount"]) <= 5000){
                        $("#modelGEHUBalanceAmountWiseSemesterRegistration").modal('show');
                    }
                }
            }
        });
    }

</script>

