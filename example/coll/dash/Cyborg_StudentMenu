                {
                    var flg= response.flg;
                    var jwts = response.jwt;
                    var oid=response.oid;
                    if(flg==0)
                    {
                        bootbox.alert("You are not permitted for Remote Access");
                        return;
                    } else if (flg == 1) {
                        bootbox.alert("Please update Your Registered Email id in Cyborg-ERP");
                        return;
                    } else if (flg == 2) {

                        $.redirect('https://uttaranchaluniversity.knimbus.com/sso/login', { 'orgId': oid, 'jwt': jwts });
                    }
                }
            });
        });


