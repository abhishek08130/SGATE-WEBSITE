<script type="text/javascript">

    $(function(){

        CheckCombinePopUp();

    });

    function CheckCombinePopUp(){

        $.ajax({
            type: "POST",
            url: '/Account/GetModalPopUpSetting',
            data: {
            },
            datatype: "json",
            success: function (response) {

                $("#dvWork").html("");
                var HtmlContent = "";

                var dt = JSON.parse(response);

                var DoneCount = 0;

                if(dt.length > 0) {

                    for(var i = 0; i < dt.length; i++)
                    {
                        var DoneNotDone = "";

                        if (parseInt(dt[i]["IsDone"]) == 0){
                            DoneNotDone = "<a href=\""+dt[i]["URL"]+"\" style=\"font-size:15px;\"><i class=\"fa fa-tasks\" style=\"cursor:pointer;\"></i>&nbsp;Click here</a>";
                        }
                        else{
                            DoneCount ++;
                            DoneNotDone = "<i class=\"fa fa-check-circle\" style=\"color:green;font-size:15px;\">&nbsp;Done</i>";
                        }

                        HtmlContent = HtmlContent +  "<p class=\"pt-3 mx-4\" style=\"font-size:15px;\">" + (i+1) +".) " +dt[i]["Caption"] + DoneNotDone + "</p>";
                        HtmlContent = HtmlContent + "<hr/>";
                    }

                    $("#dvWork").html(HtmlContent);

                    if(DoneCount == dt.length)
                    {
                        $("#ModalCombinePopUp").modal('hide');
                    }
                    else
                    {
                        $("#ModalCombinePopUp").modal('show');
                    }
                }
                else
                {
                    $("#dvWork").html("");
                    $("#ModalCombinePopUp").modal('hide');
                }

            }
        });
    }

</script>

<script type="text/javascript">

    function validateEmail(email) {
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }


    $(document).ready(function () {

        ////--------------student mobile no------------------------------------------
        ////------------------------------------------------------------------------
        $("#btnStudMobileCancel").hide();
        $("#btnStudMobileOTP").hide();
        $("#dvStuMobileOTP").hide();


        $("#txtStudMobileNo").attr("disabled", "disabled");


        $("#btnStudMobileEdit").click(function (e) {
            e.preventDefault();
            $("#txtStudMobileNo").prop("disabled", false);
            $("#btnStudMobileOTP").show();
            $("#btnStudMobileCancel").show();
            $("#btnStudMobileEdit").hide();
            $("#txtStudMobileNo").focus();
            $("#btnStudEmailEdit").prop("disabled", true);

        });

        $("#btnStudMobileCancel").click(function (e) {
            e.preventDefault();
            $("#txtStudMobileNo").val($("#tdContactNo").text());
            $("#txtStudMobileNo").prop("disabled", true);
            $("#btnStudMobileOTP").hide();
            $("#btnStudMobileEdit").show();
            $("#btnStudMobileCancel").hide();
            $("#dvStuMobileOTP").hide();
            $("#btnStuMobileCofirmOTP").hide();


            $("#btnStudEmailEdit").prop("disabled", false);
        });

        $("#btnStudMobileOTP").click(function (e) {
            e.preventDefault();

            if ($("#txtStudMobileNo").val() == '' || $("#txtStudMobileNo").val().length != 10) {
                bootbox.alert('Please enter 10 digit valid mobile no.');
                return;
            }

            var dialog = bootbox.dialog({
                message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/><b>Sending OTP, Please wait...</b></p>',
                closeButton: false
            });

            $.ajax({
                type: "POST",
                url: '/Web_StudentAcademic/SendOTP',
                data: {
                    MobileNo: $("#txtStudMobileNo").val().trim(),
                    check: "stumobile"
                },
                datatype: "json",
                success: function (response) {
                    dialog.modal('hide');

                    if (response == '1') {
                        bootbox.alert('<img src="../img/submit.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp;An OTP has been sent to your mobile no.</b>');
                        $("#btnStudMobileOTP").hide();
                        $("#txtStuMobileOTP").show();
                        $("#dvStuMobileOTP").show();
                        $("#btnStuMobileCofirmOTP").show();
                        $("#txtStudMobileNo").prop("disabled", true);
                    }
                    else  {
                        alert(response);
                    }
                }
            });
        });

        $("#btnStuMobileCofirmOTP").click(function (e) {
            e.preventDefault();

            if ($("#txtStuMobileOTP").val() == '' || $("#txtStuMobileOTP").val().length != 5) {
                bootbox.alert('Please enter 5 digit valid OTP.');
                return;
            }

            var dialog = bootbox.dialog({
                message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
                closeButton: false
            });

            $.ajax({
                type: "POST",
                url: '/Web_StudentAcademic/UpdateContact',
                data: {
                    check: "stumobile",
                    value: $("#txtStudMobileNo").val().trim(),
                    islanguage: "0",
                    OTP: $("#txtStuMobileOTP").val()
                },
                datatype: "json",
                success: function (response) {
                    dialog.modal('hide');
                    if (response == '1') {
                        bootbox.alert('<img src="../img/submit.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp; Mobile No. has been verified successully.</b>');
                        location.reload();
                    }
                    if (response == '-2') {
                        bootbox.alert('<img src="../img/failure.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp; Sorry! OTP does not match.</b>');
                        return;
                    }
                    if (response == '-3') {
                        bootbox.alert('<img src="../img/failure.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp; Something wrong in processing, please try-again.</b>');
                        return;
                    }
                    if (response == '0') {
                        bootbox.alert('<img src="../img/failure.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp; Error in processing, please contact to ERP-Cell.</b>');
                        return;
                    }
                }
            });
        });


        ////--------------student Email ID------------------------------------------
        ////------------------------------------------------------------------------
        $("#btnStudEmailCancel").hide();
        $("#btnStudEmailOTP").hide();
        $("#dvStuEmailOTP").hide();

        $("#txtStudEmail").attr("disabled", "disabled");

        $("#btnStudEmailEdit").click(function (e) {
            e.preventDefault();
            $("#txtStudEmail").prop("disabled", false);
            $("#btnStudEmailOTP").show();
            $("#btnStudEmailCancel").show();
            $("#btnStudEmailEdit").hide();
            $("#txtStudEmail").focus();

            $("#btnStudMobileEdit").prop("disabled", true);
        });

        $("#btnStudEmailCancel").click(function (e) {
            e.preventDefault();
            $("#txtStudEmail").val($("#tdEmail").text());
            $("#txtStudEmail").prop("disabled", true);
            $("#btnStudEmailOTP").hide();
            $("#btnStudEmailEdit").show();
            $("#btnStudEmailCancel").hide();

            $("#dvStuEmailOTP").hide();
            $("#btnStuEmailCofirmOTP").hide();

            $("#btnStudMobileEdit").prop("disabled", false);

        });



        $("#btnStudEmailOTP").click(function (e) {
            e.preventDefault();

            if ($("#txtStudEmail").val() == '' || !validateEmail($("#txtStudEmail").val())) {
                bootbox.alert('Please enter valid Email ID.');
                return;
            }

            var dialog = bootbox.dialog({
                message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/><b>Sending OTP, Please wait...</b></p>',
                closeButton: false
            });

            $.ajax({
                type: "POST",
                url: '/Web_StudentAcademic/SendOTP',
                data: {
                    MobileNo: $("#txtStudEmail").val().trim(),
                    check: "stuemail"
                },
                datatype: "json",
                success: function (response) {
                    dialog.modal('hide');
                    if (response == '1') {
                        bootbox.alert('<img src="../img/submit.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp;An OTP has been sent to your mentioned Email-ID.</b>');
                        $("#btnStudEmailOTP").hide();
                        $("#txtStuEmailOTP").show();
                        $("#dvStuEmailOTP").show();
                        $("#btnStuEmailCofirmOTP").show();
                        $("#txtStudEmail").prop("disabled", true);
                    }
                    if (response == '0') {
                        return;
                    }
                }
            });
        });

        $("#btnStuEmailCofirmOTP").click(function (e) {
            e.preventDefault();

            if ($("#txtStuEmailOTP").val() == '' || $("#txtStuEmailOTP").val().length != 5) {
                bootbox.alert('Please enter 5 digit valid OTP.');
                return;
            }

            var dialog = bootbox.dialog({
                message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
                closeButton: false
            });

            $.ajax({
                type: "POST",
                url: '/Web_StudentAcademic/UpdateContact',
                data: {
                    check: "stuemail",
                    value: $("#txtStudEmail").val().trim(),
                    islanguage: "0",
                    OTP: $("#txtStuEmailOTP").val()
                },
                datatype: "json",
                success: function (response) {
                    dialog.modal('hide');
                    if (response == '1') {
                        bootbox.alert('<img src="../img/submit.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp; Email-ID has been verified successully.</b>');
                        location.reload();
                    }
                    if (response == '-2') {
                        bootbox.alert('<img src="../img/failure.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp; Sorry! OTP does not match.</b>');
                        return;
                    }
                    if (response == '-3') {
                        bootbox.alert('<img src="../img/failure.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp; Something wrong in processing, please try-again.</b>');
                        return;
                    }
                    if (response == '0') {
                        bootbox.alert('<img src="../img/failure.png" style="width:30px; height:30px;"/><b> &nbsp;&nbsp; Error in processing, please contact to ERP-Cell.</b>');
                        return;
                    }
                }
            });
        });

        //////////////////////////////////////////////////////////////////////


    });




</script>

<script type="text/javascript">
    function UnReadMsg() {

        $.ajax({
            type: "POST",
            url: '/Task/UnReadMsgStu',
            data: {},
            datatype: "json",
            success: function (response) {
                if (response.Count > 0 && response.UserType==2) {
                    $("#txtbadge").show();
                    $("#txtbadge").html(response.Count);
                }
                else {
                    $("#txtbadge").hide();
                    $("#txtbadge").html('');
                }
            }
        });
    }
    function RedirectCLA(){
        var dialog = bootbox.dialog({
            message: '<p class="text-center mb-0"><img src="../Images/LoginImg/loading.svg" style="width:50px; height:50px;"/>  Please wait ...</p>',
            closeButton: false
        });

        $.ajax({
            type: "POST",
            url: '/NewStudentLogin/GetPLACLAReport',
            async: false,
            data: { StuRegisID: $("#hdnStuRegisID").val(), ProcessID: 1, Choice: 1 },
            datatype: "json",
            success: function (response) {
                dialog.modal('hide');
                if (response == 0) {
                    bootbox.alert("No record found");
                }
                else {
                    window.open('../CFOP/Cyborg_TokenGenerationViewer', '_blank');
                }
            }
        });
    }
    $(document).ready(function(){
        var Config = 'UU';

        if (Config == "QGC") {
            $("#divUnread").show();
            $("#txtbadge").hide();
            UnReadMsg();

            $("#dvFeeSubmissionLink").show();
            $('#dvFeeSubmissionLink').text('Online Fee Submission');
        }
        else {
            $("#divUnread").hide();
            $("#txtbadge").hide();
            $("#dvFeeSubmissionLink").hide();
            $('#dvFeeSubmissionLink').text('');
        }
        $("#dvQUCLAlink").click(function() {
            RedirectCLA();
        });
        $("#dvQUCLAlinkmobile").click(function() {
            RedirectCLA();
        });


    });

</script>

