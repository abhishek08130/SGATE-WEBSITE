                    //FillState(dt[0]["TCountry"], dt[0]["TState"]);
                    //FillDistrict(dt[0]["TState"], dt[0]["TDistrict"]);
                    //FillCity(dt[0]["TDistrict"], dt[0]["TTahsil"]);
                }

                /////////////////////////////////////////Certificate upload /////////////////
                if('UU' == 'UU')
                {
                    var RegID=dt[0]["RegID"];
                    FillGrid(RegID);

                    if((dt[0]["YS"]=='Semester'))
                    {
                        $('#StuYear').text("Semester:");

                    }
                    else
                    {
                        $('#StuYear').text("Year:");
                    }
                    $("#StuPhoneNo").text(dt[0]["MobileNO"]);
                    $("#StuCourse").text(dt[0]["Course"]);
                    $("#StuYearSem").text(dt[0]["YearSem"]);
                    $("#StuEmail").text(dt[0]["Email"]);
                    $("#StuName").val(dt[0]["StudentName"]);
                    $("#hdnStuRegisID").val(dt[0]["RegID"]);
                    $("#modalDraft").modal('hide');
                }


                ///////////  UU  Specialization   ////////////

                //if(dt[0]["StuSpecialization"] == "1")//
                //{
                //    $('#StudentSpecialization').modal('hide');
                //}

                ///////////////////Placement//////////////////////

                if('UU' == 'UU' && (dt[0]["StuPlacementYear"] == "1") && (dt[0]["StuPlacementRegistration"] == "0")&& (dt[0]["CollegeID"] != "32") )//
                {
                    $("#txtContactNotpa").val(dt[0]["MobileNO"]);
                    $("#txtEmailIDtpa").val(dt[0]["Email"]);
                    $("#StudentInfoplacement").modal('show');
                    $("#examplInfoplacement2").show();
                    $("#examplInfoplacement3").hide();
                }


                    /////////////QGC//////Placement//////////////////////

                else if('UU' == 'QGC' && (dt[0]["StuPlacementYear"] == "1") && (dt[0]["StuPlacementRegistration"] == "0"))//
                {
                    $("#txtContactNotpa").val(dt[0]["MobileNO"]);
                    $("#txtEmailIDtpa").val(dt[0]["Email"]);
                    $("#StudentInfoplacement").modal('show');
                    $("#examplInfoplacement2").hide();
                    $("#examplInfoplacement3").show();
                }
                else
                {

                    $("#StudentInfoplacement").modal('hide');
                }


                /////////////////////////


                if ('UU' == 'SCE')
                {
                    if (parseInt(dt[0]["BookDuesCount"]) > 0) {
                        $('#modalbookdue').modal('show');
                    }

                    $('#modalPopupSCE').modal('show');
                }



                
                ///////////////////////////////////////////////////////////////////
                /////////modal affidavit /////////////////////////////////////////
                if (('UU' == 'GEU') && (dt[0]["CollegeID"] == "1")&&(dt[0]["YS"] == "Semester")&&(dt[0]["PromotedBatch"] == "2023")&&(dt[0]["IsAffidavitSubmit"]=="0")&&((dt[0]["YearSem"] == "3")||(dt[0]["YearSem"] == "5")||(dt[0]["YearSem"] =="7")||(dt[0]["YearSem"] == "9")))
                {
                    var DOB=dt[0]["DOB"];
                    $("#hdnStuRegisID").val(dt[0]["RegID"]);
                    $("#txtEmail").text(dt[0]["Email"]);
                    $("#txtPhoneNo").text(dt[0]["MobileNO"]);
                    $("#txtAddress").text(dt[0]["PAddress"]);
                    $("#txtFatherName").text(dt[0]["FatherHusName"]);
                    $("#txtBatch").text(dt[0]["Batch"]+'.');
                    $("#txtSem").val(dt[0]["YearSem"]);

                    if (DOB != "")
                    {
                        var ServerDate = '16/11/2024';
                        var datefrom = new Date();
                        datefrom = new Date(ServerDate.split('/')[2], ServerDate.split('/')[1] - 1, ServerDate.split('/')[0]);
                        var dateto = new Date();
                        dateto = new Date(DOB.split('/')[2], DOB.split('/')[1] - 1, DOB.split('/')[0]);

                        if (datefrom > dateto) {

                            var s_month = datefrom.getMonth() + 1;
                            var s_days = datefrom.getDate();
                            var s_year = datefrom.getFullYear();

                            var d_month = dateto.getMonth() + 1;
                            var d_days = dateto.getDate();
                            var d_year = dateto.getFullYear();

                            if (s_days >= d_days) {
                                d_days = s_days - d_days;
                            }
                            else {
                                s_days = 30 + s_days;
                                s_month = s_month - 1;
                                d_days = s_days - d_days;
                            }

                            if (s_month >= d_month) {
                                d_month = s_month - d_month;
                            }
                            else {
                                s_month = 12 + s_month;
                                s_year = s_year - 1;
                                d_month = s_month - d_month;
                            }

                            d_year = s_year - d_year;

                            $("#txtAge").text(d_year);
                        }
                        else {
                            $("#txtAge").text("");
                        }
                    }

                    if(dt[0]["Gender"] == "Male")
                    {
                        $("#txtMrs").text("Mr.");
                        $("#txtDOS").text("S/O");
                    }
                    else
                    {
                        if(dt[0]["Gender"] == "Female" && dt[0]["MaritalStatus"] == "Married")
                        {
                            $("#txtMrs").text("Mrs.");
                            $("#txtDOS").text("D/O");
                        }
                        else if(dt[0]["Gender"] == "Female" && dt[0]["MaritalStatus"] == "Unmarried")
                        {
                            $("#txtMrs").text("Ms.");
                            $("#txtDOS").text("D/O");
                        }
                        else
                        {
                            $("#txtMrs").text("Ms.");
                            $("#txtDOS").text("D/O");
                        }
                    }
                    if(dt[0]["Branch"]=="")
                    {
                        $("#txtCourseBranch").text(dt[0]["Course"]);
                    }
                    else
                    {
                        $("#txtCourseBranch").text(dt[0]["Course"]+" "+dt[0]["Branch"]);
                    }

                    if(dt[0]["YearSem"]==1)
                    {
                        $("#txtSem").text(dt[0]["YearSem"]+'st');
                    }
                    if(dt[0]["YearSem"]==2)
                    {
                        $("#txtSem").text(dt[0]["YearSem"]+'nd');
                    }
                    else if(dt[0]["YearSem"]==3)
                    {
                        $("#txtSem").text(dt[0]["YearSem"]+'rd');
                    }

                    else
                    {
                        $("#txtSem").text(dt[0]["YearSem"]+'th');
                    }

                    $('#modalaffidavit').modal('show');
                }
                /////////////////////////////////////////////////////////////////
                

                var date1 = '16/11/2024';
                var date2 = '13/11/2023';

                var datefrom = date1;
                datefrom = new Date(datefrom.split('/')[2], datefrom.split('/')[1] - 1, datefrom.split('/')[0]);

                var dateto = date2;
                dateto = new Date(dateto.split('/')[2], dateto.split('/')[1] - 1, dateto.split('/')[0]);

                if (datefrom <= dateto) {

                    var Name = 'HappyDiwali.gif';

                    //$.ajax({
                    //    url: '../Account/ShowDiwaliGreeting',
                    //    datatype: "json",
                    //    type: 'POST',
                    //    data: {
                    //        Name:Name
                    //    },
                    //    success: function (response) {

                    //        $("#ImageDiwali").attr('src', '');

                    //        var dt = JSON.parse(response.state);

                    //        if (dt.length > 0){
                    //var dataURL = "data:image/jpeg;base64," + dt[0]['Attachment'];
                    ///$('#ImageDiwali').prop('src', dataURL);
                    $('#ImageDiwali').prop('src', '../Images/HappyDiwali.gif');
                    $("#ModelDiwaliGreeting").modal('show');
                    //        }
                    //    }
                    //});
                }
                //}

                //////////////////////

                //---------------Bank Of Credit--------------------

                if (('UU' == 'GEU') && (dt[0]["CollegeID"] == "1") && (dt[0]["ABCAccountNo"] == "") && (dt[0]["IsNewStudent"] == "1"))
                {
                    $('#ModelAcademicBank').modal('show');
                }
                else if (('UU' == 'GEU') && (dt[0]["CollegeID"] == "5") && (dt[0]["ABCAccountNo"] == ""))
                {
                    $('#ModelAcademicBank').modal('show');
                }
                else if('UU' == 'QGC'&&(dt[0]["ABCAccountNo"] == ""))
                {
                    $('#ModelAcademicBank').modal('show');
                }
                else
                {
                    $('#ModelAcademicBank').modal('hide');
                }


                


                if (('UU' == 'GEU' && (dt[0]["CollegeID"] == "1"|| dt[0]["CollegeID"] == "2" || dt[0]["CollegeID"] == "3" || dt[0]["CollegeID"] == "4"||dt[0]["CollegeID"] == "5"||dt[0]["CollegeID"] == "7")) && (dt[0]["UpdatePhoto"]==1)) {
                    $("#UpdatePhoto").show();

                }
                else {
                    $("#UpdatePhoto").hide();

                }

                if (dt[0]["UpdatePhoto"]== "1") {

                    $("#UpdatePhoto").show();
                }
                else {
                    $("#UpdatePhoto").hide();

                }


                //------------------------------------------------

                if (('UU' == "GEU") && parseInt(dt[0]["WorkShop"]) > 0) {
                    $("#modelGEUWorkshop").modal('show');
                }

                if ('UU' == 'GEU')
                {
                    if (parseInt(dt[0]["InternationalDocument"]) > 0)
                    {
                        $("#modelFeeInformationInternationForeign").modal('show');
                    }
                    if (dt[0]["UniversityID"] == "1")
                    {
                        if (parseInt(dt[0]["ElectiveOpt"]) == 1)
                        {
                            $("#modelOptionalSubjectToOptGEU").modal('show');
                        }
                    }
                }

                if ('UU' == 'UU' && (parseInt(dt[0]["IsMain"]) > 0 || parseInt(dt[0]["IsHostel"]) > 1)){
                    $('#modalFeeReminderUU').modal('show');
                }

                if ('UU' == 'UU' && parseInt(dt[0]["MisFeeCount"]) > 0 ){
                    $('#modelFeeNoticeUU').modal('show');
                    $('#mfnuu').text(dt[0]["MisFeeFeeHead"]);
                }

                if ('UU' == 'GEU' && dt[0]["IsNewStudent"] == '1' && dt[0]["IsApply"] == '0')
                {
                    $('#modelGEUNewStu').modal('show');
                    $('#dvgeu').show();
                    $('#dvqgc').hide();
                }
                if ('UU' == 'GEU')
                {
                    if(dt[0]["UniversityID"] == "2" ) {
                        $("#dvMsgStudentGEHU").css('display','block');

                        $("#mrqueID").text('Dear Student, IT IS ADVISED TO PAY FEE THROUGH ERP. USP of ERP fee payment portal are: 1. Instant fee receipt. 2. Automatic system generated university receipts. 3. No waiting period for offline verification. Thanks');
                    }
                    $("#dvMsgStudentAllGEUAttendance").css('display','block');
                    $("#mrqueIDAttendance").text('75% attendance is mandatory to be eligible for appearing in the end semester examinations.')
                }

                if ('UU' == 'QGC' && dt[0]["IsNewStudent"] == '1' && dt[0]["IsApply"] == '0') {
                    //$('#modelGEUNewStu').modal('show');
                    $('#dvgeu').hide();
                    $('#dvqgc').hide();
                }


                if ('UU' == 'QGC' && dt[0]["IsNewStudent"] == '1' ) {
                    if( parseInt(dt[0]["StuRegisID"]) > 0){
                        $('#lnkQUCLA').show();
                        $('#lnkQUCLAmobile').show();
                        $('#hdnStuRegisID').val(dt[0]["StuRegisID"]);
                    }
                }

                if('UU' == 'GEU' && parseInt(dt[0]["IsNewStudent"])== '1' ) {
                    ///$('#dvSelectedClassMode').text(dt[0]["ClassMode"]);
                    //setInterval(function () { blink() }, 1000);
                    //function blink() {
                    //    $("#dvSelectedClassMode").fadeTo(100, 0.1).fadeTo(200, 1.0);
                    //}
                }

                if('UU' == 'GEU') {
                    if(dt[0]["CollegeID"] == "2" || dt[0]["CollegeID"] == "3" || dt[0]["CollegeID"] == "4"){
                        //$("#dvAdditionalCourse").show();
                        //$('#dvAdditionalCourse').text('Jagar Dhol Sagar Registration');
                    }
                }


                //Instrunction Work And Other Submission for GEU   -----------  Start

                if ('UU' == 'GEU') {

                    if (parseInt(dt[0]["IsOnlyTransportMode"]) == 0 && (dt[0]["CollegeID"] == "1" || dt[0]["CollegeID"] == "2"))
                    {
                        $('#ModalHostelTransportMode').modal('show');
                        GetBusRouteInstruction();
                    }
                    else
                    {
                        $('#ModalHostelTransportMode').modal('hide');

                    }

                    //if (parseInt(dt[0]["IsOnlineOfflineMode"]) == 0)
                    //{
                    //   // $('#ModalOnlineOfflineMode').modal('show');
                    //}
                    //else
                    //{
                    //  //  $('#ModalOnlineOfflineMode').modal('hide');

                    //    if (parseInt(dt[0]["IsHostelTransportMode"]) == 0 && parseInt(dt[0]["SecondPopupShowHide"]) == 0)
                    //    {
                    //        $('#ModalHostelTransportMode').modal('show');
                    //        GetBusRouteInstruction();
                    //    }
                    //    else
                    //    {
                    //        $('#ModalHostelTransportMode').modal('hide');

                    //    }
                    //}

                    FillInstruction(dt[0]["UniversityID"],dt[0]["Univesity"],dt[0]["Course"],dt[0]["EnrollmentNo"],dt[0]["StudentName"],dt[0]["FatherHusName"]);
                }

                //Instrunction Work And Other Submission for GEU   -----------  end

                if ('37143' == '0' && 'UU' == 'GEU' &&  parseInt(dt[0]["IsTravelDetail"]) == 0 && parseInt(dt[0]["IsNewStudent"]) == 1) {
                    if (parseInt(dt[0]["IsAccomodation"]) > 0 || parseInt(dt[0]["IsHostelDues"]) > 0) {
                        $('#dvTravelDetail').modal('show');
                    } else {
                        $('#dvTravelDetail').modal('hide');
                    }
                    return;
                }else{
                    $('#dvTravelDetail').modal('hide');
                }



                if ('UU' == 'GEU') {
                    if (dt[0]["UniversityID"] == "2"){
                        FeeBalanceAmountWiseSemesterRegistration();
                    }
                }

                if ('UU' == 'DOON') {
                    if (dt[0]["CollegeID"] == "2" || dt[0]["CollegeID"] == "4" || dt[0]["CollegeID"] == "5"
                        || dt[0]["CollegeID"] == "6" || dt[0]["CollegeID"] == "7"){

                        $("#modalDcastMessage").modal('hide');
                        $("#dvHelplineNoDCast").show();
                    }
                    else
                    {
                        $("#modalDcastMessage").modal('hide');
                        $("#dvHelplineNoDCast").hide();
                    }

                    if(String(dt[0]["CollegeID"]) == "1")
                    {
                        $("#modelDOONNotice").modal('hide');
                    }
                    else
                    {
                        $("#modelDOONNotice").modal('hide');
                    }

                }
                else
                {
                    $("#modalDcastMessage").modal('hide');
                    $("#dvHelplineNoDCast").hide();
                    $("#modelDOONNotice").modal('hide');
                }

                if ( 'UU' == 'SCE')
                {
                    $("#modelReferalformSCE").modal('show');
                    if('UU' == 'QGC') {
                        if(parseInt(dt[0]["IsVaccineRemain"] && parseInt(dt[0]["IsAlumni"]) == 0) < 2) {
                            $("#modelCovidVaccine").modal('show');
                        }
                        else {
                            $("#modelCovidVaccine").modal('show')
                        }
                    } else {
                        if(parseInt(dt[0]["IsVaccineRemain"]) == 0) {
                            // $("#modelCovidVaccine").modal('show');
                            // window.location.href ='../Web_StudentAcademic/Cyborg_StudentVaccinRegistration';
                        }
                        if(parseInt(dt[0]["IsVaccineRemain"]) == 1) {
                            // $("#modelCovidVaccine").modal('show');
                        }
                    }
                }


                if ('UU' == 'GEU')
                {
                    $("#trOfficialID").show();
                    $("#tdOfficialEmailID").text(dt[0]["OfficialMailID"]);
                    $("#txtStudMobileNo").val(dt[0]["MobileNO"]);
                    $("#txtStudEmail").val(dt[0]["Email"]);
                    if (dt[0]["IsMobileVerify"] > 0)
                    {
                        $("#dvstumobilVerify").hide();
                    }
                    if(dt[0]["IsEmailVerify"] > 0){
                        $("#dvstuEmailVerify").hide();
                    }
                    if(dt[0]["IsMobileVerify"] == 0 || dt[0]["IsEmailVerify"] == 0)
                    {
                        
                    }
                }
                else
                {
                    $("#trOfficialID").hide();
                    $("#tdOfficialEmailID").text('');
                }

                if ('UU' == 'GEU') {
                    if (dt[0]["IsEnrollFormSubmit"] == "0") {
                        ///$("#modelNoticeGEU").modal('show');

                    }
                }

                if ('UU' == 'GEU' && parseInt(dt[0]["IsClassModeSelected"]) == 0
                    && parseInt(dt[0]["IsNewStudent"]) == 1  ) {
                    //$('#dvGEUOnlineOffine').modal('show');
                    ////  GetBusRoute();
                    //// return;
                } else {
                    $('#dvGEUOnlineOffine').modal('hide');
                }


                

                if ('UU' == 'GEU') {
                    if (dt[0]["UniversityID"] == "1" && dt[0]["CollegeID"] == "1" && dt[0]["Batch"] == "2021" && dt[0]["YearSem"] == "2" )
                    {
                        $('#modelYear2021_2Sem_GEU').modal('show');
                    }
                    else
                    {
                        $('#modelYear2021_2Sem_GEU').modal('hide');
                    }
                }
                else
                {
                    $('#modelYear2021_2Sem_GEU').modal('hide');
                }

                if ('UU' == 'UU') {
                    if (parseInt(dt[0]["IsTransportAssign"]) > 0){
                        $("#trStudentTransportCard").show();
                    }
                    else
                    {
                        $("#trStudentTransportCard").hide();
                    }
                }

                if('UU' == 'COER')
                {
                    $("#trABCID").show();
                }
            },
            error: function (result) {
                alert("Error");
            }
        });

        FillCircularNotices();
        FillResults();
        FillUpcomingCompany();
        FillNotification();
        Birthday();
        GetOnlineExamHome();

    });

    function redirectCousreApply(){
        window.location.href = '../Web_StudentFinance/Cyborg_AdditionalCourse_Registration';
